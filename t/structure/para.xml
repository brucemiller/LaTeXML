  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>Display Math</title>
    <para xml:id="S1.p1">
      <p>Begin first paragraph,</p>
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="a+b," text="a + b" xml:id="S1.Ex1.m1">
          <XMath>
            <XMDual>
              <XMRef idref="S1.Ex1.m1.1"/>
              <XMWrap>
                <XMApp xml:id="S1.Ex1.m1.1">
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok font="italic" role="UNKNOWN">a</XMTok>
                  <XMTok font="italic" role="UNKNOWN">b</XMTok>
                </XMApp>
                <XMTok role="PUNCT">,</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <p>continuing paragraph.</p>
    </para>
    <para xml:id="S1.p2">
      <p>Single line paragraph.</p>
    </para>
    <para xml:id="S1.p3">
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="a+b." text="a + b" xml:id="S1.Ex2.m1">
          <XMath>
            <XMDual>
              <XMRef idref="S1.Ex2.m1.1"/>
              <XMWrap>
                <XMApp xml:id="S1.Ex2.m1.1">
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok font="italic" role="UNKNOWN">a</XMTok>
                  <XMTok font="italic" role="UNKNOWN">b</XMTok>
                </XMApp>
                <XMTok role="PERIOD">.</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S1.p4">
      <p>Another single line paragraph.</p>
    </para>
  </section>
