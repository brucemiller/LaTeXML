        </equation>
        <equation xml:id="S2.Ex10">
          <MathFork>
            <Math tex="\displaystyle N=\mbox{4}" text="N = [4]" xml:id="S2.Ex10.m4">
              <XMath>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="UNKNOWN">N</XMTok>
                  <XMText class="ltx_markedasmath">4</XMText>
                </XMApp>
              </XMath>
            </Math>
            <MathBranch>
              <tr>
                <td align="right"><Math mode="inline" tex="\displaystyle N" text="N" xml:id="S2.Ex10.m1">
                    <XMath>
                      <XMTok font="italic" role="UNKNOWN">N</XMTok>
                    </XMath>
                  </Math></td>
                <td align="center"><Math mode="inline" tex="\displaystyle=" text="=" xml:id="S2.Ex10.m2">
                    <XMath>
                      <XMTok meaning="equals" role="RELOP">=</XMTok>
                    </XMath>
                  </Math></td>
                <td align="left"><text class="ltx_markedasmath">4</text></td>
              </tr>
            </MathBranch>
          </MathFork>
        </equation>
        <equation xml:id="S2.Ex11">
          <MathFork>
            <Math tex="\displaystyle N=\mbox{4}" text="N = [4]" xml:id="S2.Ex11.m4">
              <XMath>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="UNKNOWN">N</XMTok>
                  <XMText class="ltx_markedasmath">4</XMText>
