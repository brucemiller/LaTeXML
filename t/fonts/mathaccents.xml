                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMApp scriptpos="mid">
                      <XMTok name="underbrace" role="UNDERACCENT">⏟</XMTok>
                      <XMTok meaning="absent"/>
                    </XMApp>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">]</XMTok>
                </XMWrap>
              </XMDual>
              <XMHint name="quad" role="PUNCT" width="10pt"/>
              <XMDual xml:id="S0.Ex4.m1.2">
                <XMApp>
                  <XMTok meaning="delimited-[]"/>
                  <XMRef idref="S0.Ex4.m1.2.1"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">[</XMTok>
                  <XMApp xml:id="S0.Ex4.m1.2.1">
                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMApp scriptpos="mid">
                      <XMTok name="underbrace" role="UNDERACCENT">⏟</XMTok>
                      <XMTok meaning="absent"/>
                    </XMApp>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">]</XMTok>
                </XMWrap>
              </XMDual>
              <XMHint name="quad" role="PUNCT" width="10pt"/>
              <XMDual xml:id="S0.Ex4.m1.3">
                <XMApp>
                  <XMTok meaning="delimited-[]"/>
                  <XMRef idref="S0.Ex4.m1.3.1"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">[</XMTok>
                  <XMApp xml:id="S0.Ex4.m1.3.1">
                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMApp scriptpos="mid">
                      <XMTok name="underbrace" role="UNDERACCENT">⏟</XMTok>
                      <XMTok meaning="absent"/>
                    </XMApp>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">]</XMTok>
                </XMWrap>
              </XMDual>
              <XMHint name="quad" role="PUNCT" width="10pt"/>
              <XMDual xml:id="S0.Ex4.m1.4">
                <XMApp>
                  <XMTok meaning="delimited-[]"/>
                  <XMRef idref="S0.Ex4.m1.4.1"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">[</XMTok>
                  <XMApp xml:id="S0.Ex4.m1.4.1">
                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMApp scriptpos="mid">
                      <XMTok name="underbrace" role="UNDERACCENT">⏟</XMTok>
                      <XMTok meaning="absent"/>
                    </XMApp>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                  </XMApp>
