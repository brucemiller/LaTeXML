                  <XMTok role="OPEN" stretchy="false">[</XMTok>
                  <XMApp xml:id="S0.Ex1.m1.6">
                    <XMTok name="hat" role="OVERACCENT" stretchy="false">^</XMTok>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">]</XMTok>
                </XMWrap>
              </XMDual>
              <XMHint name="quad" role="PUNCT" width="10pt"/>
              <XMDual xml:id="S0.Ex1.m1.17">
                <XMApp>
                  <XMTok meaning="delimited-[]"/>
                  <XMRef idref="S0.Ex1.m1.7"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">[</XMTok>
                  <XMApp xml:id="S0.Ex1.m1.7">
                    <XMTok name="hat" role="OVERACCENT" stretchy="false">^</XMTok>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">]</XMTok>
                </XMWrap>
              </XMDual>
              <XMHint name="quad" role="PUNCT" width="10pt"/>
              <XMDual xml:id="S0.Ex1.m1.18">
                <XMApp>
                  <XMTok meaning="delimited-[]"/>
                  <XMRef idref="S0.Ex1.m1.8"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">[</XMTok>
                  <XMApp xml:id="S0.Ex1.m1.8">
                    <XMTok name="hat" role="OVERACCENT" stretchy="false">^</XMTok>
                    <XMTok font="typewriter">$</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">]</XMTok>
                </XMWrap>
              </XMDual>
              <XMHint name="quad" role="PUNCT" width="10pt"/>
              <XMDual xml:id="S0.Ex1.m1.19">
                <XMApp>
                  <XMTok meaning="delimited-[]"/>
                  <XMRef idref="S0.Ex1.m1.9"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">[</XMTok>
                  <XMApp xml:id="S0.Ex1.m1.9">
                    <XMTok name="hat" role="OVERACCENT" stretchy="false">^</XMTok>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">]</XMTok>
