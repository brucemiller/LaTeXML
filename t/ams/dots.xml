<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>Named dots</title>
    <para xml:id="S1.p1">
      <p><Math mode="inline" tex="A,B,\dotsc,Z" text="list@(A, B, dotsc, Z)" xml:id="S1.p1.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="list"/>
                <XMRef idref="S1.p1.m1.1"/>
                <XMRef idref="S1.p1.m1.2"/>
                <XMRef idref="S1.p1.m1.3"/>
                <XMRef idref="S1.p1.m1.4"/>
              </XMApp>
              <XMWrap>
                <XMTok font="italic" role="UNKNOWN" xml:id="S1.p1.m1.1">A</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMTok font="italic" role="UNKNOWN" xml:id="S1.p1.m1.2">B</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMTok name="dotsc" role="ID" xml:id="S1.p1.m1.3">…</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMTok font="italic" role="UNKNOWN" xml:id="S1.p1.m1.4">Z</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p2">
      <p><Math mode="inline" tex="A+B+\dotsb+Z" text="A + B + dotsb + Z" xml:id="S1.p2.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">A</XMTok>
              <XMTok font="italic" role="UNKNOWN">B</XMTok>
              <XMTok name="dotsb" role="ID">⋯</XMTok>
              <XMTok font="italic" role="UNKNOWN">Z</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p3">
      <p><Math mode="inline" tex="AB+\dotsm Z" text="A * B + dotsm * Z" xml:id="S1.p3.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok font="italic" role="UNKNOWN">A</XMTok>
                <XMTok font="italic" role="UNKNOWN">B</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok name="dotsm" role="ID">⋯</XMTok>
                <XMTok font="italic" role="UNKNOWN">Z</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p4">
      <p><Math mode="inline" tex="\int\dotsi dx" text="integral@(dotsi * differential-d@(x))" xml:id="S1.p4.m1">
          <XMath>
            <XMApp>
              <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok name="dotsi" role="ID">⋯</XMTok>
                <XMApp>
                  <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                  <XMTok font="italic" role="UNKNOWN">x</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p5">
      <p><Math mode="inline" tex="AB\dotso Z" text="A * B * dotso * Z" xml:id="S1.p5.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN">A</XMTok>
              <XMTok font="italic" role="UNKNOWN">B</XMTok>
              <XMTok name="dotso" role="ID">…</XMTok>
              <XMTok font="italic" role="UNKNOWN">Z</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
  </section>
  <section inlist="toc" xml:id="S2">
    <tags>
      <tag>2</tag>
      <tag role="refnum">2</tag>
      <tag role="typerefnum">§2</tag>
    </tags>
    <title><tag close=" ">2</tag>Automatic dots</title>
    <para xml:id="S2.p1">
      <p><Math mode="inline" tex="A,B,\dots,Z" text="list@(A, B, dots, Z)" xml:id="S2.p1.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="list"/>
                <XMRef idref="S2.p1.m1.1"/>
                <XMRef idref="S2.p1.m1.2"/>
                <XMRef idref="S2.p1.m1.3"/>
                <XMRef idref="S2.p1.m1.4"/>
              </XMApp>
              <XMWrap>
                <XMTok font="italic" role="UNKNOWN" xml:id="S2.p1.m1.1">A</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMTok font="italic" role="UNKNOWN" xml:id="S2.p1.m1.2">B</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMTok name="dots" role="ID" xml:id="S2.p1.m1.3">…</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMTok font="italic" role="UNKNOWN" xml:id="S2.p1.m1.4">Z</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p2">
      <p><Math mode="inline" tex="A,B,\dots Z" text="list@(A, B, dots * Z)" xml:id="S2.p2.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="list"/>
                <XMRef idref="S2.p2.m1.1"/>
                <XMRef idref="S2.p2.m1.2"/>
                <XMRef idref="S2.p2.m1.3"/>
              </XMApp>
              <XMWrap>
                <XMTok font="italic" role="UNKNOWN" xml:id="S2.p2.m1.1">A</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMTok font="italic" role="UNKNOWN" xml:id="S2.p2.m1.2">B</XMTok>
                <XMTok role="PUNCT">,</XMTok>
                <XMApp xml:id="S2.p2.m1.3">
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok name="dots" role="ID">…</XMTok>
                  <XMTok font="italic" role="UNKNOWN">Z</XMTok>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p3">
      <p><Math mode="inline" tex="A+B+\dots+Z" text="A + B + dots + Z" xml:id="S2.p3.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">A</XMTok>
              <XMTok font="italic" role="UNKNOWN">B</XMTok>
              <XMTok name="dots" role="ID">⋯</XMTok>
              <XMTok font="italic" role="UNKNOWN">Z</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p4">
      <p><Math mode="inline" tex="A+B+\dots\boldsymbol{+}Z" text="A + B + dots + Z" xml:id="S2.p4.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">A</XMTok>
              <XMTok font="italic" role="UNKNOWN">B</XMTok>
              <XMTok name="dots" role="ID">…</XMTok>
              <XMTok font="italic" role="UNKNOWN">Z</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p5">
      <p><Math mode="inline" tex="A+B+\dots-Z" text="(A + B + dots) - Z" xml:id="S2.p5.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="minus" role="ADDOP">-</XMTok>
              <XMApp>
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMTok font="italic" role="UNKNOWN">A</XMTok>
                <XMTok font="italic" role="UNKNOWN">B</XMTok>
                <XMTok name="dots" role="ID">⋯</XMTok>
              </XMApp>
              <XMTok font="italic" role="UNKNOWN">Z</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p6">
      <p><Math mode="inline" tex="A=B=\dots=Z" text="A = B = dots = Z" xml:id="S2.p6.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok font="italic" role="UNKNOWN">A</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="UNKNOWN">B</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok name="dots" role="ID">⋯</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="UNKNOWN">Z</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p7">
      <p><Math mode="inline" tex="\int\dots dx" text="integral@(dots * differential-d@(x))" xml:id="S2.p7.m1">
          <XMath>
            <XMApp>
              <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok name="dots" role="ID">…</XMTok>
                <XMApp>
                  <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                  <XMTok font="italic" role="UNKNOWN">x</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p8">
      <p><Math mode="inline" tex="AB\dots Z" text="A * B * dots * Z" xml:id="S2.p8.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN">A</XMTok>
              <XMTok font="italic" role="UNKNOWN">B</XMTok>
              <XMTok name="dots" role="ID">…</XMTok>
              <XMTok font="italic" role="UNKNOWN">Z</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p9">
      <p><Math mode="inline" tex="AB\dots" text="A * B * dots" xml:id="S2.p9.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN">A</XMTok>
              <XMTok font="italic" role="UNKNOWN">B</XMTok>
              <XMTok name="dots" role="ID">…</XMTok>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p10">
      <p><Math mode="inline" tex="\frac{A\dots}{B\dots}" text="(A * dots) / (B * dots)" xml:id="S2.p10.m1">
          <XMath>
            <XMApp>
              <XMTok mathstyle="text" meaning="divide" role="FRACOP"/>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok font="italic" fontsize="70%" role="UNKNOWN">A</XMTok>
                <XMTok fontsize="70%" name="dots" role="ID">…</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok font="italic" fontsize="70%" role="UNKNOWN">B</XMTok>
                <XMTok fontsize="70%" name="dots" role="ID">…</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math></p>
    </para>
  </section>
</document>
