  <paragraph inlist="toc" xml:id="S0.SS0.SSS0.Px1">
    <title>Normal Usage</title>
    <para xml:id="S0.SS0.SSS0.Px1.p1">
      <equationgroup class="ltx_eqn_eqnarray" xml:id="S0.EGx1">
        <equation xml:id="S0.Ex1">
          <MathFork>
            <Math tex="\displaystyle a=b+c" text="a = b + c" xml:id="S0.Ex1.m4">
              <XMath>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="UNKNOWN">a</XMTok>
                  <XMApp>
                    <XMTok meaning="plus" role="ADDOP">+</XMTok>
                    <XMTok font="italic" role="UNKNOWN">b</XMTok>
                    <XMTok font="italic" role="UNKNOWN">c</XMTok>
                  </XMApp>
                </XMApp>
              </XMath>
            </Math>
            <MathBranch>
              <tr>
                <td align="right"><Math mode="inline" tex="\displaystyle a" text="a" xml:id="S0.Ex1.m1">
                    <XMath>
                      <XMTok font="italic" role="UNKNOWN">a</XMTok>
                    </XMath>
                  </Math></td>
                <td align="center"><Math mode="inline" tex="\displaystyle=" text="=" xml:id="S0.Ex1.m2">
                    <XMath>
                      <XMTok meaning="equals" role="RELOP">=</XMTok>
                    </XMath>
                  </Math></td>
                <td align="left"><Math mode="inline" tex="\displaystyle b+c" text="b + c" xml:id="S0.Ex1.m3">
                    <XMath>
                      <XMApp>
                        <XMTok meaning="plus" role="ADDOP">+</XMTok>
                        <XMTok font="italic" role="UNKNOWN">b</XMTok>
                        <XMTok font="italic" role="UNKNOWN">c</XMTok>
                      </XMApp>
                    </XMath>
                  </Math></td>
              </tr>
            </MathBranch>
          </MathFork>
        </equation>
        <equation xml:id="S0.Ex2">
          <MathFork>
            <Math tex="\displaystyle d=e+f+g" text="d = e + f + g" xml:id="S0.Ex2.m4">
              <XMath>
                <XMApp>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="UNKNOWN">d</XMTok>
                  <XMApp>
                    <XMTok meaning="plus" role="ADDOP">+</XMTok>
                    <XMTok font="italic" role="UNKNOWN">e</XMTok>
                    <XMTok font="italic" role="UNKNOWN">f</XMTok>
                    <XMTok font="italic" role="UNKNOWN">g</XMTok>
                  </XMApp>
                </XMApp>
              </XMath>
            </Math>
            <MathBranch>
              <tr>
                <td align="right"><Math mode="inline" tex="\displaystyle d" text="d" xml:id="S0.Ex2.m1">
                    <XMath>
                      <XMTok font="italic" role="UNKNOWN">d</XMTok>
                    </XMath>
                  </Math></td>
                <td align="center"><Math mode="inline" tex="\displaystyle=" text="=" xml:id="S0.Ex2.m2">
                    <XMath>
                      <XMTok meaning="equals" role="RELOP">=</XMTok>
                    </XMath>
                  </Math></td>
                <td align="left"><Math mode="inline" tex="\displaystyle e+f+g" text="e + f + g" xml:id="S0.Ex2.m3">
                    <XMath>
                      <XMApp>
                        <XMTok meaning="plus" role="ADDOP">+</XMTok>
                        <XMTok font="italic" role="UNKNOWN">e</XMTok>
                        <XMTok font="italic" role="UNKNOWN">f</XMTok>
                        <XMTok font="italic" role="UNKNOWN">g</XMTok>
                      </XMApp>
                    </XMath>
                  </Math></td>
              </tr>
            </MathBranch>
          </MathFork>
        </equation>
      </equationgroup>
    </para>
  </paragraph>
