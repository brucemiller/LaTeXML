  <section refnum="2" xml:id="S2">
    <title><tag close=" ">2</tag>Math columns</title>
    <para xml:id="S2.p1">
      <tabular vattach="middle">
        <tbody>
          <tr>
            <td align="center" thead="true">word</td>
            <td align="center">
              <Math mode="inline" xml:id="S2.p1.m1" tex="a+b" text="a + b">
                <XMath>
                  <XMApp>
                    <XMTok meaning="plus" role="ADDOP">+</XMTok>
                    <XMTok role="UNKNOWN" font="italic">a</XMTok>
                    <XMTok role="UNKNOWN" font="italic">b</XMTok>
                  </XMApp>
                </XMath>
              </Math>
            </td>
          </tr>
          <tr>
            <td align="center" thead="true">word</td>
