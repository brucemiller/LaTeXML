    <title><tag close=" ">1</tag>Nesting hbox &amp; vbox</title>
    <para xml:id="S1.p1">
      <p>a b c d</p>
    </para>
    <para xml:id="S1.p2">
      <p>abcd</p>
    </para>
    <para xml:id="S1.p3">
      <p><inline-block vattach="bottom">
          <p>a</p>
        </inline-block><inline-block vattach="bottom">
          <p>b</p>
        </inline-block><inline-block vattach="bottom">
          <p>c</p>
        </inline-block><inline-block vattach="bottom">
          <p>d</p>
        </inline-block></p>
    </para>
    <para xml:id="S1.p4">
      <p vattach="bottom">a b c d</p>
    </para>
    <para xml:id="S1.p5">
      <inline-block vattach="bottom">
        <p>a</p>
        <p>b</p>
        <p>c</p>
        <p>d</p>
      </inline-block>
    </para>
    <para xml:id="S1.p6">
      <inline-block vattach="bottom">
        <p vattach="bottom">a</p>
        <p vattach="bottom">b</p>
        <p vattach="bottom">c</p>
        <p vattach="bottom">d</p>
      </inline-block>
    </para>
    <para xml:id="S1.p7">
      <p>ab<inline-block vattach="bottom">
          <p>a</p>
          <p>b</p>
          <p>c</p>
          <p>d</p>
        </inline-block>cd</p>
    </para>
