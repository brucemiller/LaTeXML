% change.case$ BiBTeX integration testcases
@testcase{001, 
  text    = {hello world},
  case     = {u},
  expected = {^HELLO WORLD$}
}

@testcase{002,
  text     = {HeLlo},
  case     = {u},
  expected = {^HELLO$}
}
@testcase{003,
  text     = {HeLlo}, 
  case     = {l}, 
  expected = {^hello$}
}
@testcase{004,
  text     = {HeLlo}, 
  case     = {t}, 
  expected = {^Hello$}
}
@testcase{005,
  text     = {HeLlo {WeIrD} world}, 
  case     = {u}, 
  expected = {^HELLO {WeIrD} WORLD$}
}
@testcase{006,
  text     = {HeLlo {WeIrD} world}, 
  case     = {l}, 
  expected = {^hello {WeIrD} world$}
}
@testcase{007,
