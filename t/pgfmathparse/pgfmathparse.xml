    <subsection inlist="toc" xml:id="S3.SS7">
      <tags>
        <tag>3.7</tag>
        <tag role="refnum">3.7</tag>
        <tag role="typerefnum">§3.7</tag>
      </tags>
      <title><tag close=" ">3.7</tag>94.3.7 Base conversion functions</title>
      <para xml:id="S3.SS7.p1">
        <p><text font="typewriter">hex(65535) = ffff = ffff</text></p>
      </para>
      <para xml:id="S3.SS7.p2">
        <p><text font="typewriter">Hex(65535) = FFFF = FFFF</text></p>
      </para>
      <para xml:id="S3.SS7.p3">
        <p><text font="typewriter">oct(63) = 77.0 = 77.0</text></p>
      </para>
    </subsection>
    <subsection inlist="toc" xml:id="S3.SS8">
      <tags>
        <tag>3.8</tag>
        <tag role="refnum">3.8</tag>
        <tag role="typerefnum">§3.8</tag>
      </tags>
      <title><tag close=" ">3.8</tag>94.3.8 Miscellaneous functions</title>
      <para xml:id="S3.SS8.p1">
        <p><text font="typewriter">min(3,4,-2,250,-8,100) = -8.0 = -8.0</text></p>
      </para>
      <para xml:id="S3.SS8.p2">
        <p><text font="typewriter">max(3,4,-2,250,-8,100) = 250.0 = 250.0</text></p>
      </para>
      <para xml:id="S3.SS8.p3">
        <p><text font="typewriter">veclen(12,5) = 13.0 = 13.0</text></p>
      </para>
      <para xml:id="S3.SS8.p4">
        <p><text font="typewriter">sinh(0.5) = 0.5211 = 0.5211</text></p>
      </para>
      <para xml:id="S3.SS8.p5">
        <p><text font="typewriter">cosh(0.5) = 1.12763 = 1.12763</text></p>
