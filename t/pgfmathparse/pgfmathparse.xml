    <title><tag close=" ">1</tag>94.1.1 Commands</title>
    <para xml:id="S1.p1">
      <p><text font="typewriter">2pt+3.5pt = 5.5 = 5.5</text></p>
    </para>
    <para xml:id="S1.p2">
      <p><text font="typewriter">sin(.5*pi r)*60 = 60.0 = 60.0</text></p>
    </para>
    <para xml:id="S1.p3">
      <p><text font="typewriter">1.234e+4 = 12340.0 = 12340.0</text></p>
    </para>
    <para xml:id="S1.p4">
      <p><text font="typewriter">010 = 010 = 8.0</text></p>
    </para>
    <para xml:id="S1.p5">
      <p><text font="typewriter">0xA = 10.0 = 10.0</text></p>
    </para>
    <para xml:id="S1.p6">
      <p><text font="typewriter">0XB = 11.0 = 11.0</text></p>
    </para>
    <para xml:id="S1.p7">
      <p><text font="typewriter">0b10 = 2.0 = 2.0</text></p>
    </para>
    <para xml:id="S1.p8">
      <p><text font="typewriter">0B101 = 5.0 = 5.0</text></p>
    </para>
