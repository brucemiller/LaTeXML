<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>Grouping in multi-relations</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="a=b=c=d=e" text="a = b = c = d = e" xml:id="S1.Ex1.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">b</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">c</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="a=b,c=d" text="formulae@(a = b, c = d)" xml:id="S1.Ex2.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="formulae"/>
                <XMRef idref="S1.Ex2.m1.1"/>
                <XMRef idref="S1.Ex2.m1.2"/>
              </XMApp>
              <XMWrap>
                <XMApp xml:id="S1.Ex2.m1.1">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="ID">a</XMTok>
                  <XMTok font="italic" role="ID">b</XMTok>
                </XMApp>
                <XMTok role="PUNCT">,</XMTok>
                <XMApp xml:id="S1.Ex2.m1.2">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="ID">c</XMTok>
                  <XMTok font="italic" role="ID">d</XMTok>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex3">
        <Math mode="display" tex="a=b,c,d,e" text="a = list@(b, c, d, e)" xml:id="S1.Ex3.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMDual>
                <XMApp>
                  <XMTok meaning="list"/>
                  <XMRef idref="S1.Ex3.m1.1"/>
                  <XMRef idref="S1.Ex3.m1.2"/>
                  <XMRef idref="S1.Ex3.m1.3"/>
                  <XMRef idref="S1.Ex3.m1.4"/>
                </XMApp>
                <XMWrap>
                  <XMTok font="italic" role="ID" xml:id="S1.Ex3.m1.1">b</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok font="italic" role="ID" xml:id="S1.Ex3.m1.2">c</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok font="italic" role="ID" xml:id="S1.Ex3.m1.3">d</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok font="italic" role="ID" xml:id="S1.Ex3.m1.4">e</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex4">
        <Math mode="display" tex="a=b,c,d=e" text="formulae@(a = list@(b, c), d = e)" xml:id="S1.Ex4.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="formulae"/>
                <XMRef idref="S1.Ex4.m1.3"/>
                <XMRef idref="S1.Ex4.m1.4"/>
              </XMApp>
              <XMWrap>
                <XMApp xml:id="S1.Ex4.m1.3">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="ID">a</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="list"/>
                      <XMRef idref="S1.Ex4.m1.1"/>
                      <XMRef idref="S1.Ex4.m1.2"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok font="italic" role="ID" xml:id="S1.Ex4.m1.1">b</XMTok>
                      <XMTok role="PUNCT">,</XMTok>
                      <XMTok font="italic" role="ID" xml:id="S1.Ex4.m1.2">c</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMTok role="PUNCT">,</XMTok>
                <XMApp xml:id="S1.Ex4.m1.4">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="ID">d</XMTok>
                  <XMTok font="italic" role="ID">e</XMTok>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex5">
        <Math mode="display" tex="a=b&gt;c\leq d=e" text="a = b &gt; c &lt;= d = e" xml:id="S1.Ex5.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">b</XMTok>
              <XMTok meaning="greater-than" role="RELOP">&gt;</XMTok>
              <XMTok font="italic" role="ID">c</XMTok>
              <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
              <XMTok font="italic" role="ID">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <paragraph inlist="toc" xml:id="S1.SS0.SSS0.Px1">
      <title>But period is somewhat different!</title>
      <para xml:id="S1.SS0.SSS0.Px1.p1">
        <equation xml:id="S1.Ex6">
          <Math mode="display" tex="a=b,c.d=e" text="formulae@(a = list@(b, c), d = e)" xml:id="S1.Ex6.m1">
            <XMath>
              <XMDual>
                <XMApp>
                  <XMTok meaning="formulae"/>
                  <XMRef idref="S1.Ex6.m1.3"/>
                  <XMRef idref="S1.Ex6.m1.4"/>
                </XMApp>
                <XMWrap>
                  <XMApp xml:id="S1.Ex6.m1.3">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok font="italic" role="ID">a</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                        <XMRef idref="S1.Ex6.m1.1"/>
                        <XMRef idref="S1.Ex6.m1.2"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok font="italic" role="ID" xml:id="S1.Ex6.m1.1">b</XMTok>
                        <XMTok role="PUNCT">,</XMTok>
                        <XMTok font="italic" role="ID" xml:id="S1.Ex6.m1.2">c</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMApp>
                  <XMTok role="PERIOD">.</XMTok>
                  <XMApp xml:id="S1.Ex6.m1.4">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok font="italic" role="ID">d</XMTok>
                    <XMTok font="italic" role="ID">e</XMTok>
                  </XMApp>
                </XMWrap>
              </XMDual>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex7">
          <Math mode="display" tex="a=b.c,d=e" text="formulae@(a = b, list@(c, d) = e)" xml:id="S1.Ex7.m1">
            <XMath>
              <XMDual>
                <XMApp>
                  <XMTok meaning="formulae"/>
                  <XMRef idref="S1.Ex7.m1.3"/>
                  <XMRef idref="S1.Ex7.m1.4"/>
                </XMApp>
                <XMWrap>
                  <XMApp xml:id="S1.Ex7.m1.3">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMTok font="italic" role="ID">a</XMTok>
                    <XMTok font="italic" role="ID">b</XMTok>
                  </XMApp>
                  <XMTok role="PERIOD">.</XMTok>
                  <XMApp xml:id="S1.Ex7.m1.4">
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                        <XMRef idref="S1.Ex7.m1.1"/>
                        <XMRef idref="S1.Ex7.m1.2"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok font="italic" role="ID" xml:id="S1.Ex7.m1.1">c</XMTok>
                        <XMTok role="PUNCT">,</XMTok>
                        <XMTok font="italic" role="ID" xml:id="S1.Ex7.m1.2">d</XMTok>
                      </XMWrap>
                    </XMDual>
                    <XMTok font="italic" role="ID">e</XMTok>
                  </XMApp>
                </XMWrap>
              </XMDual>
            </XMath>
          </Math>
        </equation>
      </para>
    </paragraph>
  </section>
  <section inlist="toc" xml:id="S2">
    <tags>
      <tag>2</tag>
      <tag role="refnum">2</tag>
      <tag role="typerefnum">§2</tag>
    </tags>
    <title><tag close=" ">2</tag>Arrows as meta-relation</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex8">
        <Math mode="display" tex="a=b\rightarrow c\leq d=e" text="a = b rightarrow c &lt;= d = e" xml:id="S2.Ex8.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="multirelation"/>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">b</XMTok>
              <XMTok name="rightarrow" role="ARROW">→</XMTok>
              <XMTok font="italic" role="ID">c</XMTok>
              <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
              <XMTok font="italic" role="ID">d</XMTok>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex9">
        <Math mode="display" tex="a=b,c\rightarrow d=e" text="formulae@(a = b, c rightarrow d = e)" xml:id="S2.Ex9.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="formulae"/>
                <XMRef idref="S2.Ex9.m1.1"/>
                <XMRef idref="S2.Ex9.m1.2"/>
              </XMApp>
              <XMWrap>
                <XMApp xml:id="S2.Ex9.m1.1">
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="ID">a</XMTok>
                  <XMTok font="italic" role="ID">b</XMTok>
                </XMApp>
                <XMTok role="PUNCT">,</XMTok>
                <XMApp xml:id="S2.Ex9.m1.2">
                  <XMTok meaning="multirelation"/>
                  <XMTok font="italic" role="ID">c</XMTok>
                  <XMTok name="rightarrow" role="ARROW">→</XMTok>
                  <XMTok font="italic" role="ID">d</XMTok>
                  <XMTok meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" role="ID">e</XMTok>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex10">
        <Math mode="display" tex="a=b\leftrightarrow c\leq d=e" text="a = b leftrightarrow c &lt;= d = e" xml:id="S2.Ex10.m1">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok font="italic" role="ID">a</XMTok>
                <XMTok font="italic" role="ID">b</XMTok>
              </XMApp>
              <XMApp>
                <XMTok meaning="multirelation"/>
                <XMTok font="italic" role="ID">c</XMTok>
                <XMTok meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
                <XMTok font="italic" role="ID">d</XMTok>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok font="italic" role="ID">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex11">
        <Math mode="display" tex="a=b,c\leftrightarrow d=e" text="a = list@(b, c) leftrightarrow d = e" xml:id="S2.Ex11.m1">
          <XMath>
            <XMApp>
              <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok font="italic" role="ID">a</XMTok>
                <XMDual>
                  <XMApp>
                    <XMTok meaning="list"/>
                    <XMRef idref="S2.Ex11.m1.1"/>
                    <XMRef idref="S2.Ex11.m1.2"/>
                  </XMApp>
                  <XMWrap>
                    <XMTok font="italic" role="ID" xml:id="S2.Ex11.m1.1">b</XMTok>
                    <XMTok role="PUNCT">,</XMTok>
                    <XMTok font="italic" role="ID" xml:id="S2.Ex11.m1.2">c</XMTok>
                  </XMWrap>
                </XMDual>
              </XMApp>
              <XMApp>
                <XMTok meaning="equals" role="RELOP">=</XMTok>
                <XMTok font="italic" role="ID">d</XMTok>
                <XMTok font="italic" role="ID">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section inlist="toc" xml:id="S3">
    <tags>
      <tag>3</tag>
      <tag role="refnum">3</tag>
      <tag role="typerefnum">§3</tag>
    </tags>
    <title><tag close=" ">3</tag>Other Meta relations</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex12">
        <Math mode="display" tex="a(&lt;e)" text="annotated@(a, absent &lt; e)" xml:id="S3.Ex12.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="annotated"/>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMDual>
                <XMRef idref="S3.Ex12.m1.1"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S3.Ex12.m1.1">
                    <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                    <XMTok meaning="absent"/>
                    <XMTok font="italic" role="ID">e</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex13">
        <Math mode="display" tex="a;(&lt;e)" text="annotated@(a, absent &lt; e)" xml:id="S3.Ex13.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="annotated"/>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMDual>
                <XMRef idref="S3.Ex13.m1.1"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S3.Ex13.m1.1">
                    <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                    <XMTok meaning="absent"/>
                    <XMTok font="italic" role="ID">e</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex14">
        <Math mode="display" tex="a=b+c+d(&lt;e)" text="a = annotated@(b + c + d, absent &lt; e)" xml:id="S3.Ex14.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMApp>
                <XMTok meaning="annotated"/>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok font="italic" role="ID">b</XMTok>
                  <XMTok font="italic" role="ID">c</XMTok>
                  <XMTok font="italic" role="ID">d</XMTok>
                </XMApp>
                <XMDual>
                  <XMRef idref="S3.Ex14.m1.1"/>
                  <XMWrap>
                    <XMTok role="OPEN" stretchy="false">(</XMTok>
                    <XMApp xml:id="S3.Ex14.m1.1">
                      <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                      <XMTok meaning="absent"/>
                      <XMTok font="italic" role="ID">e</XMTok>
                    </XMApp>
                    <XMTok role="CLOSE" stretchy="false">)</XMTok>
                  </XMWrap>
                </XMDual>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex15">
        <Math mode="display" tex="a=b+c+d\mod e" text="a = modulo@(b + c + d, e)" xml:id="S3.Ex15.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" role="ID">a</XMTok>
              <XMApp>
                <XMTok meaning="modulo" role="MODIFIEROP">mod</XMTok>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok font="italic" role="ID">b</XMTok>
                  <XMTok font="italic" role="ID">c</XMTok>
                  <XMTok font="italic" role="ID">d</XMTok>
                </XMApp>
                <XMTok font="italic" role="ID">e</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
