  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="\{a\}" text="set@(a)" xml:id="S0.Ex1.m1">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="set"/>
              <XMRef idref="S0.Ex1.m1.1"/>
            </XMApp>
            <XMWrap>
              <XMTok role="OPEN" stretchy="false">{</XMTok>
              <XMTok font="italic" role="ID" xml:id="S0.Ex1.m1.1">a</XMTok>
              <XMTok role="CLOSE" stretchy="false">}</XMTok>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="\{a:a\in b\}" text="conditional-set@(a, a element-of b)" xml:id="S0.Ex2.m1">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="conditional-set"/>
              <XMRef idref="S0.Ex2.m1.1"/>
              <XMRef idref="S0.Ex2.m1.2"/>
            </XMApp>
            <XMWrap>
              <XMTok role="OPEN" stretchy="false">{</XMTok>
              <XMTok font="italic" role="ID" xml:id="S0.Ex2.m1.1">a</XMTok>
              <XMTok name="colon" role="METARELOP">:</XMTok>
              <XMApp xml:id="S0.Ex2.m1.2">
                <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
                <XMTok font="italic" role="ID">a</XMTok>
                <XMTok font="italic" role="ID">b</XMTok>
              </XMApp>
              <XMTok role="CLOSE" stretchy="false">}</XMTok>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex3">
      <Math mode="display" tex="\{a|a\in b\}" text="conditional-set@(a, a element-of b)" xml:id="S0.Ex3.m1">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="conditional-set"/>
              <XMRef idref="S0.Ex3.m1.1"/>
              <XMRef idref="S0.Ex3.m1.2"/>
            </XMApp>
            <XMWrap>
              <XMTok role="OPEN" stretchy="false">{</XMTok>
              <XMTok font="italic" role="ID" xml:id="S0.Ex3.m1.1">a</XMTok>
              <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              <XMApp xml:id="S0.Ex3.m1.2">
                <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
                <XMTok font="italic" role="ID">a</XMTok>
                <XMTok font="italic" role="ID">b</XMTok>
              </XMApp>
              <XMTok role="CLOSE" stretchy="false">}</XMTok>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex4">
      <Math mode="display" tex="\{a|a\in b\}_{c=0}" text="(conditional-set@(a, a element-of b)) _ (c = 0)" xml:id="S0.Ex4.m1">
        <XMath>
          <XMApp>
            <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
            <XMDual>
              <XMApp>
                <XMTok meaning="conditional-set"/>
                <XMRef idref="S0.Ex4.m1.1"/>
                <XMRef idref="S0.Ex4.m1.2"/>
              </XMApp>
              <XMWrap>
                <XMTok role="OPEN" stretchy="false">{</XMTok>
                <XMTok font="italic" role="ID" xml:id="S0.Ex4.m1.1">a</XMTok>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="S0.Ex4.m1.2">
                  <XMTok meaning="element-of" name="in" role="RELOP">∈</XMTok>
                  <XMTok font="italic" role="ID">a</XMTok>
                  <XMTok font="italic" role="ID">b</XMTok>
                </XMApp>
                <XMTok role="CLOSE" stretchy="false">}</XMTok>
              </XMWrap>
            </XMDual>
            <XMApp>
              <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
              <XMTok font="italic" fontsize="70%" role="ID">c</XMTok>
              <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
