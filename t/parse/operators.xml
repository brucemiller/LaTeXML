                    <XMTok role="UNKNOWN" xml:id="S1.Ex1.m1.1.2.1.1" font="italic">a</XMTok>
                    <XMTok meaning="0" role="NUMBER" xml:id="S1.Ex1.m1.1.2.1.3">0</XMTok>
                  </XMApp>
                </XMApp>
                <XMTok meaning="infinity" name="infty" role="ID" xml:id="S1.Ex1.m1.1.3.1">∞</XMTok>
              </XMApp>
              <XMApp xml:id="S1.Ex1.m1.1.10.2">
                <XMTok meaning="times" role="MULOP" xml:id="S1.Ex1.m1.1.10.2.1">⁢</XMTok>
                <XMApp xml:id="S1.Ex1.m1.1.10.2.2">
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2" xml:id="S1.Ex1.m1.1.10.2.2.1"/>
                  <XMTok possibleFunction="yes" role="UNKNOWN" xml:id="S1.Ex1.m1.1.4" font="italic">f</XMTok>
                  <XMTok role="UNKNOWN" xml:id="S1.Ex1.m1.1.5.1" font="italic">a</XMTok>
                </XMApp>
                <XMApp close=")" open="(" xml:id="S1.Ex1.m1.1.10.2.3">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2" xml:id="S1.Ex1.m1.1.10.2.3.1"/>
