    <title><tag close=" ">4</tag>Logic operators</title>
    <para xml:id="S4.p1">
      <equation xml:id="S4.Ex15">
        <Math mode="display" tex="\exists x.P(x)" text="formulae@(exists@(x), P * x)" xml:id="S4.Ex15.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="formulae"/>
                <XMRef idref="S4.Ex15.m1.2"/>
                <XMRef idref="S4.Ex15.m1.3"/>
              </XMApp>
              <XMWrap>
                <XMApp xml:id="S4.Ex15.m1.2">
                  <XMTok meaning="exists" role="BIGOP">∃</XMTok>
                  <XMTok font="italic" role="UNKNOWN">x</XMTok>
                </XMApp>
                <XMTok role="PERIOD">.</XMTok>
                <XMApp xml:id="S4.Ex15.m1.3">
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN">P</XMTok>
                  <XMDual>
                    <XMRef idref="S4.Ex15.m1.1"/>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMTok font="italic" role="UNKNOWN" xml:id="S4.Ex15.m1.1">x</XMTok>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
