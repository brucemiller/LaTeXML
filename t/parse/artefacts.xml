<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="stmaryrd"?>
<?latexml package="latexml"?>
<?latexml package="amsmath"?>
<?latexml package="array"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>Edge/Artefact Cases</title>
    <para xml:id="S1.p1">
      <enumerate xml:id="S1.I1">
        <item xml:id="S1.I1.i1">
          <tags>
            <tag>1.</tag>
            <tag role="refnum">1</tag>
            <tag role="typerefnum">item 1</tag>
          </tags>
          <para xml:id="S1.I1.i1.p1">
            <p><Math mode="inline" tex="\," text="absent" xml:id="S1.I1.i1.p1.m1">
                <XMath>
                  <XMTok meaning="absent"/>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i2">
          <tags>
            <tag>2.</tag>
            <tag role="refnum">2</tag>
            <tag role="typerefnum">item 2</tag>
          </tags>
          <para xml:id="S1.I1.i2.p1">
            <p><Math mode="inline" tex="\times x" text="absent * x" xml:id="S1.I1.i2.p1.m1">
                <XMath>
                  <XMApp>
                    <XMTok meaning="times" role="MULOP">×</XMTok>
                    <XMTok meaning="absent"/>
                    <XMTok font="italic" role="UNKNOWN">x</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i3">
          <tags>
            <tag>3.</tag>
            <tag role="refnum">3</tag>
            <tag role="typerefnum">item 3</tag>
          </tags>
          <para xml:id="S1.I1.i3.p1">
            <p><Math mode="inline" tex="\mathbin{|}x" text="|@(x)" xml:id="S1.I1.i3.p1.m1">
                <XMath>
                  <XMApp>
                    <XMTok role="BINOP" stretchy="false">|</XMTok>
                    <XMTok font="italic" role="UNKNOWN">x</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i4">
          <tags>
            <tag>4.</tag>
            <tag role="refnum">4</tag>
            <tag role="typerefnum">item 4</tag>
          </tags>
          <para xml:id="S1.I1.i4.p1">
            <p><Math mode="inline" tex="\rightarrow." text="rightarrow" xml:id="S1.I1.i4.p1.m1">
                <XMath>
                  <XMDual>
                    <XMRef idref="S1.I1.i4.p1.m1.1"/>
                    <XMWrap>
                      <XMTok name="rightarrow" role="ARROW" xml:id="S1.I1.i4.p1.m1.1">→</XMTok>
                      <XMTok role="PERIOD">.</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i5">
          <tags>
            <tag>5.</tag>
            <tag role="refnum">5</tag>
            <tag role="typerefnum">item 5</tag>
          </tags>
          <para xml:id="S1.I1.i5.p1">
            <p><Math mode="inline" tex="\rightarrow," text="rightarrow" xml:id="S1.I1.i5.p1.m1">
                <XMath>
                  <XMDual>
                    <XMRef idref="S1.I1.i5.p1.m1.1"/>
                    <XMWrap>
                      <XMTok name="rightarrow" role="ARROW" xml:id="S1.I1.i5.p1.m1.1">→</XMTok>
                      <XMTok role="PUNCT">,</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i6">
          <tags>
            <tag>6.</tag>
            <tag role="refnum">6</tag>
            <tag role="typerefnum">item 6</tag>
          </tags>
          <para xml:id="S1.I1.i6.p1">
            <p><Math mode="inline" tex="\left.xyz\right|_{0}^{2}x" text="evaluated-at@(x * y * z, 0, 2) * x" xml:id="S1.I1.i6.p1.m1">
                <XMath>
                  <XMApp>
                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="evaluated-at"/>
                        <XMRef idref="S1.I1.i6.p1.m1.3"/>
                        <XMRef idref="S1.I1.i6.p1.m1.1"/>
                        <XMRef idref="S1.I1.i6.p1.m1.2"/>
                      </XMApp>
                      <XMApp>
                        <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                        <XMApp>
                          <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                          <XMWrap>
                            <XMApp xml:id="S1.I1.i6.p1.m1.3">
                              <XMTok meaning="times" role="MULOP">⁢</XMTok>
                              <XMTok font="italic" role="UNKNOWN">x</XMTok>
                              <XMTok font="italic" role="UNKNOWN">y</XMTok>
                              <XMTok font="italic" role="UNKNOWN">z</XMTok>
                            </XMApp>
                            <XMTok role="CLOSE" stretchy="true">|</XMTok>
                          </XMWrap>
                          <XMTok fontsize="70%" meaning="0" role="NUMBER" xml:id="S1.I1.i6.p1.m1.1">0</XMTok>
                        </XMApp>
                        <XMTok fontsize="70%" meaning="2" role="NUMBER" xml:id="S1.I1.i6.p1.m1.2">2</XMTok>
                      </XMApp>
                    </XMDual>
                    <XMTok font="italic" role="UNKNOWN">x</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i7">
          <tags>
            <tag>7.</tag>
            <tag role="refnum">7</tag>
            <tag role="typerefnum">item 7</tag>
          </tags>
          <para xml:id="S1.I1.i7.p1">
            <p><Math mode="inline" tex="A\boxslash B" text="boxslash@(A, B)" xml:id="S1.I1.i7.p1.m1">
                <XMath>
                  <XMApp>
                    <XMTok name="boxslash" role="BINOP">⧄</XMTok>
                    <XMTok font="italic" role="UNKNOWN">A</XMTok>
                    <XMTok font="italic" role="UNKNOWN">B</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i8">
          <tags>
            <tag>8.</tag>
            <tag role="refnum">8</tag>
            <tag role="typerefnum">item 8</tag>
          </tags>
          <para xml:id="S1.I1.i8.p1">
            <p><Math mode="inline" tex="x\times_{i}^{2}y" text="x (* _ i) ^ 2 y" xml:id="S1.I1.i8.p1.m1">
                <XMath>
                  <XMApp>
                    <XMApp meaning="times" role="MULOP">
                      <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                      <XMApp meaning="times" role="MULOP">
                        <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                        <XMTok meaning="times" role="MULOP">×</XMTok>
                        <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                      </XMApp>
                      <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
                    </XMApp>
                    <XMTok font="italic" role="UNKNOWN">x</XMTok>
                    <XMTok font="italic" role="UNKNOWN">y</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i9">
          <tags>
            <tag>9.</tag>
            <tag role="refnum">9</tag>
            <tag role="typerefnum">item 9</tag>
          </tags>
          <para xml:id="S1.I1.i9.p1">
            <p><Math mode="inline" tex="|\rightarrow\rangle" text="ket@(rightarrow)" xml:id="S1.I1.i9.p1.m1">
                <XMath>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="ket"/>
                      <XMRef idref="S1.I1.i9.p1.m1.1"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">|</XMTok>
                      <XMTok name="rightarrow" role="ARROW" xml:id="S1.I1.i9.p1.m1.1">→</XMTok>
                      <XMTok name="rangle" role="CLOSE" stretchy="false">⟩</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i10">
          <tags>
            <tag>10.</tag>
            <tag role="refnum">10</tag>
            <tag role="typerefnum">item 10</tag>
          </tags>
          <para xml:id="S1.I1.i10.p1">
            <p><Math mode="inline" tex="|\iff\rangle" text="ket@(iff)" xml:id="S1.I1.i10.p1.m1">
                <XMath>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="ket"/>
                      <XMRef idref="S1.I1.i10.p1.m1.1"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">|</XMTok>
                      <XMTok meaning="iff" role="METARELOP" xml:id="S1.I1.i10.p1.m1.1">⇔</XMTok>
                      <XMTok name="rangle" role="CLOSE" stretchy="false">⟩</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i11">
          <tags>
            <tag>11.</tag>
            <tag role="refnum">11</tag>
            <tag role="typerefnum">item 11</tag>
          </tags>
          <para xml:id="S1.I1.i11.p1">
            <p><Math mode="inline" tex="|\bmod\rangle" text="ket@(modulo)" xml:id="S1.I1.i11.p1.m1">
                <XMath>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="ket"/>
                      <XMRef idref="S1.I1.i11.p1.m1.1"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">|</XMTok>
                      <XMTok meaning="modulo" name="bmod" role="MODIFIEROP" xml:id="S1.I1.i11.p1.m1.1">mod</XMTok>
                      <XMTok name="rangle" role="CLOSE" stretchy="false">⟩</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i12">
          <tags>
            <tag>12.</tag>
            <tag role="refnum">12</tag>
            <tag role="typerefnum">item 12</tag>
          </tags>
          <para xml:id="S1.I1.i12.p1">
            <p><Math mode="inline" tex="|\times_{i}^{2}\rangle" text="ket@((* _ i) ^ 2)" xml:id="S1.I1.i12.p1.m1">
                <XMath>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="ket"/>
                      <XMRef idref="S1.I1.i12.p1.m1.1"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">|</XMTok>
                      <XMApp meaning="times" role="MULOP" xml:id="S1.I1.i12.p1.m1.1">
                        <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                        <XMApp meaning="times" role="MULOP">
                          <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                          <XMTok meaning="times" role="MULOP">×</XMTok>
                          <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                        </XMApp>
                        <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
                      </XMApp>
                      <XMTok name="rangle" role="CLOSE" stretchy="false">⟩</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i13">
          <tags>
            <tag>13.</tag>
            <tag role="refnum">13</tag>
            <tag role="typerefnum">item 13</tag>
          </tags>
          <para xml:id="S1.I1.i13.p1">
            <p><Math mode="inline" tex="\sin\pi|x" text="conditional@(sine@(pi), x)" xml:id="S1.I1.i13.p1.m1">
                <XMath>
                  <XMApp>
                    <XMTok meaning="conditional" role="MODIFIEROP" stretchy="false">|</XMTok>
                    <XMApp>
                      <XMTok meaning="sine" role="TRIGFUNCTION">sin</XMTok>
                      <XMTok font="italic" name="pi" role="UNKNOWN">π</XMTok>
                    </XMApp>
                    <XMTok font="italic" role="UNKNOWN">x</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i14">
          <tags>
            <tag>14.</tag>
            <tag role="refnum">14</tag>
            <tag role="typerefnum">item 14</tag>
          </tags>
          <para xml:id="S1.I1.i14.p1">
            <p><Math mode="inline" tex="\partial\sin x\times_{i}^{2}y" text="(partial-differential@(sine))@(x (* _ i) ^ 2 y)" xml:id="S1.I1.i14.p1.m1">
                <XMath>
                  <XMApp>
                    <XMApp>
                      <XMTok meaning="partial-differential" name="partial" role="OPERATOR">∂</XMTok>
                      <XMTok meaning="sine" role="TRIGFUNCTION">sin</XMTok>
                    </XMApp>
                    <XMApp>
                      <XMApp meaning="times" role="MULOP">
                        <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                        <XMApp meaning="times" role="MULOP">
                          <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                          <XMTok meaning="times" role="MULOP">×</XMTok>
                          <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                        </XMApp>
                        <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
                      </XMApp>
                      <XMTok font="italic" role="UNKNOWN">x</XMTok>
                      <XMTok font="italic" role="UNKNOWN">y</XMTok>
                    </XMApp>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i15">
          <tags>
            <tag>15.</tag>
            <tag role="refnum">15</tag>
            <tag role="typerefnum">item 15</tag>
          </tags>
          <para xml:id="S1.I1.i15.p1">
            <p><Math mode="inline" tex="(\int)" text="integral" xml:id="S1.I1.i15.p1.m1">
                <XMath>
                  <XMDual>
                    <XMRef idref="S1.I1.i15.p1.m1.1"/>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP" xml:id="S1.I1.i15.p1.m1.1">∫</XMTok>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i16">
          <tags>
            <tag>16.</tag>
            <tag role="refnum">16</tag>
            <tag role="typerefnum">item 16</tag>
          </tags>
          <para xml:id="S1.I1.i16.p1">
            <p><Math mode="inline" tex="(\Delta)" text="Delta" xml:id="S1.I1.i16.p1.m1">
                <XMath>
                  <XMDual>
                    <XMRef idref="S1.I1.i16.p1.m1.1"/>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMTok name="Delta" role="DIFFOP" xml:id="S1.I1.i16.p1.m1.1">Δ</XMTok>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i17">
          <tags>
            <tag>17.</tag>
            <tag role="refnum">17</tag>
            <tag role="typerefnum">item 17</tag>
          </tags>
          <para xml:id="S1.I1.i17.p1">
            <p><Math mode="inline" tex="{}^{x}{}_{y}f" text="^ x _ y f" xml:id="S1.I1.i17.p1.m1">
                <XMath>
                  <XMApp>
                    <XMTok role="SUPERSCRIPTOP" scriptpos="pre2"/>
                    <XMApp>
                      <XMTok role="SUBSCRIPTOP" scriptpos="pre1"/>
                      <XMTok font="italic" role="UNKNOWN">f</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">y</XMTok>
                    </XMApp>
                    <XMTok font="italic" fontsize="70%" role="UNKNOWN">x</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
        <item xml:id="S1.I1.i18">
          <tags>
            <tag>18.</tag>
            <tag role="refnum">18</tag>
            <tag role="typerefnum">item 18</tag>
          </tags>
          <para xml:id="S1.I1.i18.p1">
            <p><Math mode="inline" tex="\begin{array}[]{cc}F&amp;G\end{array}\begin{array}[]{cc}x&amp;y\end{array}" text="Array[[F, G]]@(Array[[x, y]])" xml:id="S1.I1.i18.p1.m1">
                <XMath>
                  <XMApp>
                    <XMArray role="ARRAY" vattach="middle">
                      <XMRow>
                        <XMCell align="center">
                          <XMTok font="italic" role="UNKNOWN">F</XMTok>
                        </XMCell>
                        <XMCell align="center">
                          <XMTok font="italic" role="UNKNOWN">G</XMTok>
                        </XMCell>
                      </XMRow>
                    </XMArray>
                    <XMArray role="ARRAY" vattach="middle">
                      <XMRow>
                        <XMCell align="center">
                          <XMTok font="italic" role="UNKNOWN">x</XMTok>
                        </XMCell>
                        <XMCell align="center">
                          <XMTok font="italic" role="UNKNOWN">y</XMTok>
                        </XMCell>
                      </XMRow>
                    </XMArray>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
      </enumerate>
    </para>
  </section>
</document>
