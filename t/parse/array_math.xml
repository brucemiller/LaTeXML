  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="\begin{cases}{cc}d&amp;c\\&#10;b&amp;a\end{cases}" text="cases@(c * c * d, c, b, a)" xml:id="S0.Ex1.m1">
