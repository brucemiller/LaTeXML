                <XMRow>
                  <XMCell align="left">
                    <XMTok font="italic" role="UNKNOWN" xml:id="S0.Ex1.m1.3">b</XMTok>
                  </XMCell>
                  <XMCell align="left">
                    <XMTok font="italic" role="UNKNOWN" xml:id="S0.Ex1.m1.4">a</XMTok>
                  </XMCell>
                </XMRow>
              </XMArray>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
  </para>
