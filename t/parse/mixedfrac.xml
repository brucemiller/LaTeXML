  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>No fraction</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="a+2b" text="a + 2 * b" xml:id="S1.Ex1.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="2" role="NUMBER">2</XMTok>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="a+123.456b" text="a + 123.456 * b" xml:id="S1.Ex2.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="123.456" role="NUMBER">123.456</XMTok>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section inlist="toc" xml:id="S2">
    <tags>
      <tag>2</tag>
      <tag role="refnum">2</tag>
      <tag role="typerefnum">§2</tag>
    </tags>
    <title><tag close=" ">2</tag>Mixed</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex3">
        <Math mode="display" tex="1\frac{1}{2}" text="1 + 1 / 2" xml:id="S2.Ex3.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">⁤</XMTok>
              <XMTok meaning="1" role="NUMBER">1</XMTok>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                <XMTok meaning="1" role="NUMBER">1</XMTok>
                <XMTok meaning="2" role="NUMBER">2</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex4">
        <Math mode="display" tex="a+123\frac{12}{34}b" text="a + (123 + 12 / 34) * b" xml:id="S2.Ex4.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMApp>
                  <XMTok meaning="plus" role="ADDOP">⁤</XMTok>
                  <XMTok meaning="123" role="NUMBER">123</XMTok>
                  <XMApp>
                    <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                    <XMTok meaning="12" role="NUMBER">12</XMTok>
                    <XMTok meaning="34" role="NUMBER">34</XMTok>
                  </XMApp>
                </XMApp>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section inlist="toc" xml:id="S3">
    <tags>
      <tag>3</tag>
      <tag role="refnum">3</tag>
      <tag role="typerefnum">§3</tag>
    </tags>
    <title><tag close=" ">3</tag>Not Mixed</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex5">
        <Math mode="display" tex="a+123\frac{12.0}{34}b" text="a + 123 * (12.0 / 34) * b" xml:id="S3.Ex5.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="123" role="NUMBER">123</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                  <XMTok meaning="12.0" role="NUMBER">12.0</XMTok>
                  <XMTok meaning="34" role="NUMBER">34</XMTok>
                </XMApp>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex6">
        <Math mode="display" tex="a+123\frac{12}{y}b" text="a + 123 * (12 / y) * b" xml:id="S3.Ex6.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="123" role="NUMBER">123</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                  <XMTok meaning="12" role="NUMBER">12</XMTok>
                  <XMTok font="italic" role="UNKNOWN">y</XMTok>
                </XMApp>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex7">
        <Math mode="display" tex="a+123\frac{12x}{y}b" text="a + 123 * ((12 * x) / y) * b" xml:id="S3.Ex7.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="123" role="NUMBER">123</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                  <XMApp>
                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMTok meaning="12" role="NUMBER">12</XMTok>
                    <XMTok font="italic" role="UNKNOWN">x</XMTok>
                  </XMApp>
                  <XMTok font="italic" role="UNKNOWN">y</XMTok>
                </XMApp>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex8">
        <Math mode="display" tex="a+123\frac{x}{y}b" text="a + 123 * (x / y) * b" xml:id="S3.Ex8.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="123" role="NUMBER">123</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                  <XMTok font="italic" role="UNKNOWN">x</XMTok>
                  <XMTok font="italic" role="UNKNOWN">y</XMTok>
                </XMApp>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex9">
        <Math mode="display" tex="a+123.456\frac{12}{34}b" text="a + 123.456 * (12 / 34) * b" xml:id="S3.Ex9.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="123.456" role="NUMBER">123.456</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                  <XMTok meaning="12" role="NUMBER">12</XMTok>
                  <XMTok meaning="34" role="NUMBER">34</XMTok>
                </XMApp>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex10">
        <Math mode="display" tex="a+2b\frac{1}{3}" text="a + 2 * b * (1 / 3)" xml:id="S3.Ex10.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok meaning="2" role="NUMBER">2</XMTok>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
                <XMApp>
                  <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                  <XMTok meaning="1" role="NUMBER">1</XMTok>
                  <XMTok meaning="3" role="NUMBER">3</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex11">
        <Math mode="display" tex="2\frac{(-1)^{m}}{(2m)!}" text="2 * ((- 1) ^ m / (2 * m)factorial)" xml:id="S3.Ex11.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok meaning="2" role="NUMBER">2</XMTok>
              <XMApp>
                <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post3"/>
                  <XMDual>
                    <XMRef idref="S3.Ex11.m1.1"/>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMApp xml:id="S3.Ex11.m1.1">
                        <XMTok meaning="minus" role="ADDOP">-</XMTok>
                        <XMTok meaning="1" role="NUMBER">1</XMTok>
                      </XMApp>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">m</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="factorial" role="POSTFIX">!</XMTok>
                  <XMDual>
                    <XMRef idref="S3.Ex11.m1.2"/>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMApp xml:id="S3.Ex11.m1.2">
                        <XMTok meaning="times" role="MULOP">⁢</XMTok>
                        <XMTok meaning="2" role="NUMBER">2</XMTok>
                        <XMTok font="italic" role="UNKNOWN">m</XMTok>
                      </XMApp>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
