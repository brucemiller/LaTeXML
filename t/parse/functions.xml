  <section inlist="toc" xml:id="S7">
    <tags>
      <tag>7</tag>
      <tag role="refnum">7</tag>
      <tag role="typerefnum">§7</tag>
    </tags>
    <title><tag close=" ">7</tag>Advanced argument patterns</title>
    <para xml:id="S7.p1">
      <equation xml:id="S7.Ex23">
        <Math mode="display" tex="f(a&lt;b)" text="f@(a less b)" xml:id="S7.Ex23.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S7.Ex23.m1.1"/>
                <XMRef idref="S7.Ex23.m1.2"/>
              </XMApp>
              <XMApp>
                <XMTok font="italic" role="FUNCTION" xml:id="S7.Ex23.m1.1">f</XMTok>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S7.Ex23.m1.2">
                    <XMTok meaning="less-than" role="RELOP">&lt;</XMTok>
                    <XMTok font="italic" role="ID">a</XMTok>
                    <XMTok font="italic" role="ID">b</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S7.Ex24">
        <Math mode="display" tex="f(a:b)" text="f@(a colon b)" xml:id="S7.Ex24.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S7.Ex24.m1.1"/>
                <XMRef idref="S7.Ex24.m1.2"/>
              </XMApp>
              <XMApp>
                <XMTok font="italic" role="FUNCTION" xml:id="S7.Ex24.m1.1">f</XMTok>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S7.Ex24.m1.2">
                    <XMTok name="colon" role="METARELOP">:</XMTok>
                    <XMTok font="italic" role="ID">a</XMTok>
                    <XMTok font="italic" role="ID">b</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S7.Ex25">
        <Math mode="display" tex="f(a\leftrightarrow b)" text="f@(a leftrightarrow b)" xml:id="S7.Ex25.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S7.Ex25.m1.1"/>
                <XMRef idref="S7.Ex25.m1.2"/>
              </XMApp>
              <XMApp>
                <XMTok font="italic" role="FUNCTION" xml:id="S7.Ex25.m1.1">f</XMTok>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S7.Ex25.m1.2">
                    <XMTok name="leftrightarrow" role="METARELOP">↔</XMTok>
                    <XMTok font="italic" role="ID">a</XMTok>
                    <XMTok font="italic" role="ID">b</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S7.Ex26">
        <Math mode="display" tex="f(\rightarrow,\leftarrow)" text="f@(rightarrow, leftarrow)" xml:id="S7.Ex26.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S7.Ex26.m1.1"/>
                <XMRef idref="S7.Ex26.m1.2"/>
                <XMRef idref="S7.Ex26.m1.3"/>
              </XMApp>
              <XMApp>
                <XMTok font="italic" role="FUNCTION" xml:id="S7.Ex26.m1.1">f</XMTok>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok name="rightarrow" role="ARROW" xml:id="S7.Ex26.m1.2">→</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok name="leftarrow" role="ARROW" xml:id="S7.Ex26.m1.3">←</XMTok>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
