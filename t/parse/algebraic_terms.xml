<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <para xml:id="p1">
    <p><Math mode="inline" tex="xyz" text="x * y * z" xml:id="p1.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok font="italic" role="UNKNOWN">x</XMTok>
            <XMTok font="italic" role="UNKNOWN">y</XMTok>
            <XMTok font="italic" role="UNKNOWN">z</XMTok>
          </XMApp>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p2">
    <p><Math mode="inline" tex="\infty+\pi" text="infinity + pi" xml:id="p2.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
            <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
            <XMTok font="italic" name="pi" role="UNKNOWN">π</XMTok>
          </XMApp>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p3">
    <p><Math mode="inline" tex="2\frac{3}{4}" text="2 + 3 / 4" xml:id="p3.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">⁤</XMTok>
            <XMTok meaning="2" role="NUMBER">2</XMTok>
            <XMApp>
              <XMTok mathstyle="text" meaning="divide" role="FRACOP"/>
              <XMTok fontsize="70%" meaning="3" role="NUMBER">3</XMTok>
              <XMTok fontsize="70%" meaning="4" role="NUMBER">4</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p4">
    <p><Math mode="inline" tex="a+" text="limit-from@(a, +)" xml:id="p4.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="limit-from"/>
            <XMTok font="italic" role="UNKNOWN">a</XMTok>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
          </XMApp>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p5">
    <p><Math mode="inline" tex="-xy" text="- x * y" xml:id="p5.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="minus" role="ADDOP">-</XMTok>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN">x</XMTok>
              <XMTok font="italic" role="UNKNOWN">y</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p6">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="ab+cd-ef+gh" text="((a * b + c * d) - e * f) + g * h" xml:id="S0.Ex1.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
            <XMApp>
              <XMTok meaning="minus" role="ADDOP">-</XMTok>
              <XMApp>
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN">a</XMTok>
                  <XMTok font="italic" role="UNKNOWN">b</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN">c</XMTok>
                  <XMTok font="italic" role="UNKNOWN">d</XMTok>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok font="italic" role="UNKNOWN">e</XMTok>
                <XMTok font="italic" role="UNKNOWN">f</XMTok>
              </XMApp>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN">g</XMTok>
              <XMTok font="italic" role="UNKNOWN">h</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p7">
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="ab+c^{2}d^{3}-ef^{5}+g^{-1}h" text="((a * b + c ^ 2 * d ^ 3) - e * f ^ 5) + g ^ (- 1) * h" xml:id="S0.Ex2.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
            <XMApp>
              <XMTok meaning="minus" role="ADDOP">-</XMTok>
              <XMApp>
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN">a</XMTok>
                  <XMTok font="italic" role="UNKNOWN">b</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMApp>
                    <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                    <XMTok font="italic" role="UNKNOWN">c</XMTok>
                    <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
                  </XMApp>
                  <XMApp>
                    <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                    <XMTok font="italic" role="UNKNOWN">d</XMTok>
                    <XMTok fontsize="70%" meaning="3" role="NUMBER">3</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMTok font="italic" role="UNKNOWN">e</XMTok>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                  <XMTok font="italic" role="UNKNOWN">f</XMTok>
                  <XMTok fontsize="70%" meaning="5" role="NUMBER">5</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                <XMTok font="italic" role="UNKNOWN">g</XMTok>
                <XMApp>
                  <XMTok fontsize="70%" meaning="minus" role="ADDOP">-</XMTok>
                  <XMTok fontsize="70%" meaning="1" role="NUMBER">1</XMTok>
                </XMApp>
              </XMApp>
              <XMTok font="italic" role="UNKNOWN">h</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
</document>
