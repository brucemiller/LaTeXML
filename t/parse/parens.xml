    <title><tag close=" ">3</tag>Cases</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex7">
        <Math mode="display" tex="\left.\begin{array}[]{l}a\\&#10;b\end{array}\right\}=\left\{\begin{array}[]{l}c\\&#10;d\end{array}\right." text="cases@(Array[[a], [b]]) = cases@(Array[[c], [d]])" xml:id="S3.Ex7.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMDual>
                <XMApp>
                  <XMTok meaning="cases"/>
                  <XMRef idref="S3.Ex7.m1.1"/>
                </XMApp>
                <XMWrap>
                  <XMTok meaning="absent"/>
                  <XMArray role="ARRAY" vattach="middle" xml:id="S3.Ex7.m1.1">
                    <XMRow>
                      <XMCell align="left">
                        <XMTok font="italic" role="ID">a</XMTok>
                      </XMCell>
                    </XMRow>
                    <XMRow>
                      <XMCell align="left">
                        <XMTok font="italic" role="ID">b</XMTok>
                      </XMCell>
                    </XMRow>
                  </XMArray>
                  <XMTok role="CLOSE" stretchy="true">}</XMTok>
                </XMWrap>
              </XMDual>
              <XMDual>
                <XMApp>
                  <XMTok meaning="cases"/>
                  <XMRef idref="S3.Ex7.m1.2"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="true">{</XMTok>
                  <XMArray role="ARRAY" vattach="middle" xml:id="S3.Ex7.m1.2">
                    <XMRow>
                      <XMCell align="left">
                        <XMTok font="italic" role="ID">c</XMTok>
                      </XMCell>
                    </XMRow>
                    <XMRow>
                      <XMCell align="left">
                        <XMTok font="italic" role="ID">d</XMTok>
                      </XMCell>
                    </XMRow>
                  </XMArray>
                  <XMTok meaning="absent"/>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S3.p2">
