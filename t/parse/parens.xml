    <title><tag close=" ">1</tag>Basic math</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="((a+b)-c)" text="(a + b) - c" xml:id="S1.Ex1.m1">
          <XMath>
            <XMDual>
              <XMRef idref="S1.Ex1.m1.1"/>
              <XMWrap>
                <XMTok role="OPEN" stretchy="false">(</XMTok>
                <XMApp xml:id="S1.Ex1.m1.1">
                  <XMTok meaning="minus" role="ADDOP">-</XMTok>
                  <XMDual>
                    <XMRef idref="S1.Ex1.m1.1.1"/>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMApp xml:id="S1.Ex1.m1.1.1">
                        <XMTok meaning="plus" role="ADDOP">+</XMTok>
                        <XMTok font="italic" role="ID">a</XMTok>
                        <XMTok font="italic" role="ID">b</XMTok>
                      </XMApp>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                  <XMTok font="italic" role="ID">c</XMTok>
                </XMApp>
                <XMTok role="CLOSE" stretchy="false">)</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="(a+(b-c))" text="a + (b - c)" xml:id="S1.Ex2.m1">
          <XMath>
            <XMDual>
              <XMRef idref="S1.Ex2.m1.1"/>
              <XMWrap>
                <XMTok role="OPEN" stretchy="false">(</XMTok>
                <XMApp xml:id="S1.Ex2.m1.1">
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok font="italic" role="ID">a</XMTok>
                  <XMDual>
                    <XMRef idref="S1.Ex2.m1.1.1"/>
                    <XMWrap>
                      <XMTok role="OPEN" stretchy="false">(</XMTok>
                      <XMApp xml:id="S1.Ex2.m1.1.1">
                        <XMTok meaning="minus" role="ADDOP">-</XMTok>
                        <XMTok font="italic" role="ID">b</XMTok>
                        <XMTok font="italic" role="ID">c</XMTok>
                      </XMApp>
                      <XMTok role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMTok role="CLOSE" stretchy="false">)</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
