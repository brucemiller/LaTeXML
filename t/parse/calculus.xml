  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="\int_{0}^{\infty}x^{2}dx" text="((integral _ 0) ^ infinity)@(x ^ 2 * differential-d@(x))" xml:id="S0.Ex1.m1">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
              </XMApp>
              <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                <XMTok font="italic" role="UNKNOWN">x</XMTok>
                <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
              </XMApp>
              <XMApp>
                <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                <XMTok font="italic" role="UNKNOWN">x</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="\int_{0}^{\infty}x^{2}d\mathord{x}" text="((integral _ 0) ^ infinity)@(x ^ 2 * differential-d@(x))" xml:id="S0.Ex2.m1">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
              </XMApp>
              <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
                <XMTok font="italic" role="UNKNOWN">x</XMTok>
                <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
              </XMApp>
              <XMApp>
                <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                <XMTok font="italic" role="ID">x</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p3">
    <equation xml:id="S0.Ex3">
      <Math mode="display" tex="\int_{\infty}^{(0+)}t\,dt" text="((integral _ infinity) ^ (limit-from@(0, +)))@(t * differential-d@(t))" xml:id="S0.Ex3.m1">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
              </XMApp>
              <XMDual>
                <XMRef idref="S0.Ex3.m1.1"/>
                <XMWrap>
                  <XMTok fontsize="70%" role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S0.Ex3.m1.1">
                    <XMTok meaning="limit-from"/>
                    <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                    <XMTok fontsize="70%" meaning="plus" role="ADDOP">+</XMTok>
                  </XMApp>
                  <XMTok fontsize="70%" role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt">t</XMTok>
              <XMApp>
                <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                <XMTok font="italic" role="UNKNOWN">t</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p4">
    <equation xml:id="S0.Ex4">
      <Math mode="display" tex="\int_{\infty}^{(1+,0+,1-,0-)}t\,dt" text="((integral _ infinity) ^ (vector@(limit-from@(1, +), limit-from@(0, +), limit-from@(1, -), limit-from@(0, -))))@(t * differential-d@(t))" xml:id="S0.Ex4.m1">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
              </XMApp>
              <XMDual>
                <XMApp>
                  <XMTok meaning="vector"/>
                  <XMRef idref="S0.Ex4.m1.1"/>
                  <XMRef idref="S0.Ex4.m1.2"/>
                  <XMRef idref="S0.Ex4.m1.3"/>
                  <XMRef idref="S0.Ex4.m1.4"/>
                </XMApp>
                <XMWrap>
                  <XMTok fontsize="70%" role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S0.Ex4.m1.1">
                    <XMTok meaning="limit-from"/>
                    <XMTok fontsize="70%" meaning="1" role="NUMBER">1</XMTok>
                    <XMTok fontsize="70%" meaning="plus" role="ADDOP">+</XMTok>
                  </XMApp>
                  <XMTok fontsize="70%" role="PUNCT">,</XMTok>
                  <XMApp xml:id="S0.Ex4.m1.2">
                    <XMTok meaning="limit-from"/>
                    <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                    <XMTok fontsize="70%" meaning="plus" role="ADDOP">+</XMTok>
                  </XMApp>
                  <XMTok fontsize="70%" role="PUNCT">,</XMTok>
                  <XMApp xml:id="S0.Ex4.m1.3">
                    <XMTok meaning="limit-from"/>
                    <XMTok fontsize="70%" meaning="1" role="NUMBER">1</XMTok>
                    <XMTok fontsize="70%" meaning="minus" role="ADDOP">-</XMTok>
                  </XMApp>
                  <XMTok fontsize="70%" role="PUNCT">,</XMTok>
                  <XMApp xml:id="S0.Ex4.m1.4">
                    <XMTok meaning="limit-from"/>
                    <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                    <XMTok fontsize="70%" meaning="minus" role="ADDOP">-</XMTok>
                  </XMApp>
                  <XMTok fontsize="70%" role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt">t</XMTok>
              <XMApp>
                <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                <XMTok font="italic" role="UNKNOWN">t</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p5">
    <equation xml:id="S0.Ex5">
      <Math mode="display" tex="\sum_{i=0}^{50}x^{i}" text="((sum _ (i = 0)) ^ 50)@(x ^ i)" xml:id="S0.Ex5.m1">
        <XMath>
          <XMApp>
            <XMApp scriptpos="mid">
              <XMTok role="SUPERSCRIPTOP" scriptpos="mid1"/>
              <XMApp scriptpos="mid">
                <XMTok role="SUBSCRIPTOP" scriptpos="mid1"/>
                <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                <XMApp>
                  <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                  <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                </XMApp>
              </XMApp>
              <XMTok fontsize="70%" meaning="50" role="NUMBER">50</XMTok>
            </XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">x</XMTok>
              <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p6">
    <p><Math mode="inline" tex="\left.y\right|_{0}" text="evaluated-at@(y, 0)" xml:id="p6.m1">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="evaluated-at"/>
              <XMRef idref="p6.m1.1"/>
              <XMRef idref="p6.m1.2"/>
            </XMApp>
            <XMApp>
              <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
              <XMWrap>
                <XMTok font="italic" role="UNKNOWN" xml:id="p6.m1.1">y</XMTok>
                <XMTok role="CLOSE" stretchy="true">|</XMTok>
              </XMWrap>
              <XMTok fontsize="70%" meaning="0" role="NUMBER" xml:id="p6.m1.2">0</XMTok>
            </XMApp>
          </XMDual>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p7">
    <p><Math mode="inline" tex="\left.xyz\right|_{0}" text="evaluated-at@(x * y * z, 0)" xml:id="p7.m1">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="evaluated-at"/>
              <XMRef idref="p7.m1.2"/>
              <XMRef idref="p7.m1.1"/>
            </XMApp>
            <XMApp>
              <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
              <XMWrap>
                <XMApp xml:id="p7.m1.2">
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN">x</XMTok>
                  <XMTok font="italic" role="UNKNOWN">y</XMTok>
                  <XMTok font="italic" role="UNKNOWN">z</XMTok>
                </XMApp>
                <XMTok role="CLOSE" stretchy="true">|</XMTok>
              </XMWrap>
              <XMTok fontsize="70%" meaning="0" role="NUMBER" xml:id="p7.m1.1">0</XMTok>
            </XMApp>
          </XMDual>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p8">
    <equation xml:id="S0.Ex6">
      <Math mode="display" tex="\sum_{\scriptstyle 0\leq i\leq m\atop\scriptstyle 0&lt;j&lt;n}P(i,j)" text="(sum _ ((0 less= i less= m) FRACOP (0 less j less n)))@(P * open-interval@(i, j))" xml:id="S0.Ex6.m1">
        <XMath>
          <XMApp>
            <XMApp scriptpos="mid">
              <XMTok role="SUBSCRIPTOP" scriptpos="mid1"/>
              <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
              <XMApp>
                <XMTok mathstyle="script" role="FRACOP" thickness="0pt"/>
                <XMApp>
                  <XMTok meaning="multirelation"/>
                  <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                  <XMTok fontsize="70%" meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                  <XMTok fontsize="70%" meaning="less-than-or-equals" name="leq" role="RELOP">≤</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">m</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="multirelation"/>
                  <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                  <XMTok fontsize="70%" meaning="less-than" role="RELOP">&lt;</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">j</XMTok>
                  <XMTok fontsize="70%" meaning="less-than" role="RELOP">&lt;</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">n</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN">P</XMTok>
              <XMDual>
                <XMApp>
                  <XMTok meaning="open-interval"/>
                  <XMRef idref="S0.Ex6.m1.1"/>
                  <XMRef idref="S0.Ex6.m1.2"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok font="italic" role="UNKNOWN" xml:id="S0.Ex6.m1.1">i</XMTok>
                  <XMTok role="PUNCT">,</XMTok>
                  <XMTok font="italic" role="UNKNOWN" xml:id="S0.Ex6.m1.2">j</XMTok>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p9">
    <equation xml:id="S0.Ex7">
      <Math mode="display" tex="\sum_{i=1}^{p}\sum_{j=1}^{q}\sum_{k=1}^{r}a_{ij}b_{jk}c_{ki}" text="((sum _ (i = 1)) ^ p)@(((sum _ (j = 1)) ^ q)@(((sum _ (k = 1)) ^ r)@(a _ (i * j) * b _ (j * k) * c _ (k * i))))" xml:id="S0.Ex7.m1">
        <XMath>
          <XMApp>
            <XMApp scriptpos="mid">
              <XMTok role="SUPERSCRIPTOP" scriptpos="mid1"/>
              <XMApp scriptpos="mid">
                <XMTok role="SUBSCRIPTOP" scriptpos="mid1"/>
                <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                <XMApp>
                  <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                  <XMTok fontsize="70%" meaning="1" role="NUMBER">1</XMTok>
                </XMApp>
              </XMApp>
              <XMTok font="italic" fontsize="70%" role="UNKNOWN">p</XMTok>
            </XMApp>
            <XMApp>
              <XMApp scriptpos="mid">
                <XMTok role="SUPERSCRIPTOP" scriptpos="mid1"/>
                <XMApp scriptpos="mid">
                  <XMTok role="SUBSCRIPTOP" scriptpos="mid1"/>
                  <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                  <XMApp>
                    <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
                    <XMTok font="italic" fontsize="70%" role="UNKNOWN">j</XMTok>
                    <XMTok fontsize="70%" meaning="1" role="NUMBER">1</XMTok>
                  </XMApp>
                </XMApp>
                <XMTok font="italic" fontsize="70%" role="UNKNOWN">q</XMTok>
              </XMApp>
              <XMApp>
                <XMApp scriptpos="mid">
                  <XMTok role="SUPERSCRIPTOP" scriptpos="mid1"/>
                  <XMApp scriptpos="mid">
                    <XMTok role="SUBSCRIPTOP" scriptpos="mid1"/>
                    <XMTok mathstyle="display" meaning="sum" role="SUMOP" scriptpos="mid">∑</XMTok>
                    <XMApp>
                      <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">k</XMTok>
                      <XMTok fontsize="70%" meaning="1" role="NUMBER">1</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">r</XMTok>
                </XMApp>
                <XMApp>
                  <XMTok meaning="times" role="MULOP">⁢</XMTok>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                    <XMTok font="italic" role="UNKNOWN">a</XMTok>
                    <XMApp>
                      <XMTok meaning="times" role="MULOP">⁢</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">j</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                    <XMTok font="italic" role="UNKNOWN">b</XMTok>
                    <XMApp>
                      <XMTok meaning="times" role="MULOP">⁢</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">j</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">k</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                    <XMTok font="italic" role="UNKNOWN">c</XMTok>
                    <XMApp>
                      <XMTok meaning="times" role="MULOP">⁢</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">k</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">i</XMTok>
                    </XMApp>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p10">
    <equation xml:id="S0.Ex8">
      <Math mode="display" tex="x|^{a}_{y=0}" text="evaluated-at@(x, y = 0, a)" xml:id="S0.Ex8.m1">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="evaluated-at"/>
              <XMRef idref="S0.Ex8.m1.1"/>
              <XMRef idref="S0.Ex8.m1.2"/>
              <XMRef idref="S0.Ex8.m1.3"/>
            </XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                <XMWrap>
                  <XMTok font="italic" role="UNKNOWN" xml:id="S0.Ex8.m1.1">x</XMTok>
                  <XMTok role="CLOSE" stretchy="false">|</XMTok>
                </XMWrap>
                <XMApp xml:id="S0.Ex8.m1.2">
                  <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN">y</XMTok>
                  <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                </XMApp>
              </XMApp>
              <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S0.Ex8.m1.3">a</XMTok>
            </XMApp>
          </XMDual>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p11">
    <equation xml:id="S0.Ex9">
      <Math mode="display" tex="\int F\times Gdx" text="integral@(F * G * differential-d@(x))" xml:id="S0.Ex9.m1">
        <XMath>
          <XMApp>
            <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">×</XMTok>
                <XMTok font="italic" role="UNKNOWN">F</XMTok>
                <XMTok font="italic" role="UNKNOWN">G</XMTok>
              </XMApp>
              <XMApp>
                <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                <XMTok font="italic" role="UNKNOWN">x</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
