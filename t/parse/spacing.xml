<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="latexml" options="mathparserspeculate"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="abc" text="a * b * c" xml:id="S0.Ex1.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok font="italic" role="UNKNOWN">a</XMTok>
            <XMTok font="italic" role="UNKNOWN">b</XMTok>
            <XMTok font="italic" role="UNKNOWN">c</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="a\,b\,c" text="a * b * c" xml:id="S0.Ex2.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt">a</XMTok>
            <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt">b</XMTok>
            <XMTok font="italic" role="UNKNOWN">c</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex3">
      <Math mode="display" tex="a+bc+d" text="a + b * c + d" xml:id="S0.Ex3.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
            <XMTok font="italic" role="UNKNOWN">a</XMTok>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" role="UNKNOWN">b</XMTok>
              <XMTok font="italic" role="UNKNOWN">c</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">d</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex4">
      <Math mode="display" tex="a+b\quad c+d" text="list@(a + b, c + d)" xml:id="S0.Ex4.m1">
        <XMath>
          <XMDual>
            <XMApp>
              <XMTok meaning="list"/>
              <XMRef idref="S0.Ex4.m1.1"/>
              <XMRef idref="S0.Ex4.m1.2"/>
            </XMApp>
            <XMWrap>
              <XMApp xml:id="S0.Ex4.m1.1">
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMTok font="italic" role="UNKNOWN">a</XMTok>
                <XMTok font="italic" role="UNKNOWN">b</XMTok>
              </XMApp>
              <XMTok font="italic" name="quad" role="PUNCT"> </XMTok>
              <XMApp xml:id="S0.Ex4.m1.2">
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMTok font="italic" role="UNKNOWN">c</XMTok>
                <XMTok font="italic" role="UNKNOWN">d</XMTok>
              </XMApp>
            </XMWrap>
          </XMDual>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p3">
    <equation xml:id="S0.Ex5">
      <Math mode="display" tex="\int_{0}^{\infty}f(x)dx" text="((integral _ 0) ^ infinity)@(f * x * differential-d@(x))" xml:id="S0.Ex5.m1">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
              </XMApp>
              <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" possibleFunction="yes" role="UNKNOWN">f</XMTok>
              <XMDual>
                <XMRef idref="S0.Ex5.m1.1"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok font="italic" role="UNKNOWN" xml:id="S0.Ex5.m1.1">x</XMTok>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
              <XMApp>
                <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                <XMTok font="italic" role="UNKNOWN">x</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex6">
      <Math mode="display" tex="\int_{0}^{\infty}f(x)\,dx" text="((integral _ 0) ^ infinity)@(f * x * differential-d@(x))" xml:id="S0.Ex6.m1">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post1"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
              </XMApp>
              <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok font="italic" possibleFunction="yes" role="UNKNOWN">f</XMTok>
              <XMDual>
                <XMRef idref="S0.Ex6.m1.1"/>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMTok font="italic" role="UNKNOWN" xml:id="S0.Ex6.m1.1">x</XMTok>
                  <XMTok role="CLOSE" rpadding="1.7pt" stretchy="false">)</XMTok>
                </XMWrap>
              </XMDual>
              <XMApp>
                <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                <XMTok font="italic" role="UNKNOWN">x</XMTok>
              </XMApp>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p4">
    <equation xml:id="S0.Ex7">
      <Math mode="display" tex="a^{2}b" text="a ^ 2 * b" xml:id="S0.Ex7.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">b</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex8">
      <Math mode="display" tex="a^{2}\,b" text="a ^ 2 * b" xml:id="S0.Ex8.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMApp rpadding="1.7pt">
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">b</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p5">
    <equation xml:id="S0.Ex9">
      <Math mode="display" tex="a\frac{b}{c}d" text="a * (b / c) * d" xml:id="S0.Ex9.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok font="italic" role="UNKNOWN">a</XMTok>
            <XMApp>
              <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
              <XMTok font="italic" role="UNKNOWN">b</XMTok>
              <XMTok font="italic" role="UNKNOWN">c</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">d</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex10">
      <Math mode="display" tex="a\,\frac{b}{c}\,d" text="a * (b / c) * d" xml:id="S0.Ex10.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt">a</XMTok>
            <XMApp rpadding="1.7pt">
              <XMTok mathstyle="display" meaning="divide" role="FRACOP"/>
              <XMTok font="italic" role="UNKNOWN">b</XMTok>
              <XMTok font="italic" role="UNKNOWN">c</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">d</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
</document>
