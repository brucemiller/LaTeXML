              <XMTok meaning="2" role="NUMBER">2</XMTok>
            </XMApp>
            <XMTok role="UNKNOWN" font="italic">b</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex8">
      <Math mode="display" xml:id="S0.Ex8.m1" tex="a^{2}\,b" text="a ^ 2 * b">
