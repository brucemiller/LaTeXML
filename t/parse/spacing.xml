  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" xml:id="S0.Ex1.m1" tex="abc" text="a * b * c">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok role="UNKNOWN" font="italic">a</XMTok>
            <XMTok role="UNKNOWN" font="italic">b</XMTok>
            <XMTok role="UNKNOWN" font="italic">c</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex2">
      <Math mode="display" xml:id="S0.Ex2.m1" tex="a\,b\,c" text="a * b * c">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok role="UNKNOWN" rpadding="1.67pt" font="italic">a</XMTok>
            <XMTok role="UNKNOWN" rpadding="1.67pt" font="italic">b</XMTok>
            <XMTok role="UNKNOWN" font="italic">c</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex3">
      <Math mode="display" xml:id="S0.Ex3.m1" tex="a+bc+d" text="a + b * c + d">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
            <XMTok role="UNKNOWN" font="italic">a</XMTok>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok role="UNKNOWN" font="italic">b</XMTok>
              <XMTok role="UNKNOWN" font="italic">c</XMTok>
            </XMApp>
            <XMTok role="UNKNOWN" font="italic">d</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex4">
      <Math mode="display" xml:id="S0.Ex4.m1" tex="a+b\quad c+d" text="list@(a + b, c + d)">
        <XMath>
          <XMApp>
            <XMTok meaning="list" role="FENCED" separators=" "/>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" font="italic">a</XMTok>
              <XMTok role="UNKNOWN" font="italic">b</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" font="italic">c</XMTok>
              <XMTok role="UNKNOWN" font="italic">d</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p3">
    <equation xml:id="S0.Ex5">
      <Math mode="display" xml:id="S0.Ex5.m1" tex="\int_{0}^{\infty}f(x)dx" text="((integral _ 0) ^ infinity)@(f * x * d * x)">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok meaning="0" role="NUMBER">0</XMTok>
              </XMApp>
              <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok possibleFunction="yes" role="UNKNOWN" font="italic">f</XMTok>
              <XMTok close=")" open="(" role="UNKNOWN" font="italic">x</XMTok>
              <XMTok role="UNKNOWN" font="italic">d</XMTok>
              <XMTok role="UNKNOWN" font="italic">x</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
    <equation xml:id="S0.Ex6">
      <Math mode="display" xml:id="S0.Ex6.m1" tex="\int_{0}^{\infty}f(x)\,dx" text="((integral _ 0) ^ infinity)@(f * x * d * x)">
        <XMath>
          <XMApp>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMTok meaning="0" role="NUMBER">0</XMTok>
              </XMApp>
              <XMTok meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMTok possibleFunction="yes" role="UNKNOWN" font="italic">f</XMTok>
              <XMTok close=")" open="(" role="UNKNOWN" rpadding="1.67pt" font="italic">x</XMTok>
              <XMTok role="UNKNOWN" font="italic">d</XMTok>
              <XMTok role="UNKNOWN" font="italic">x</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p4">
