            <XMWrap>
              <XMTok role="OPEN" stretchy="false">(</XMTok>
              <XMDual xml:id="p1.m1.5">
                <XMApp>
                  <XMTok meaning="cases"/>
                  <XMRef idref="p1.m1.1"/>
                  <XMRef idref="p1.m1.2"/>
                  <XMRef idref="p1.m1.3"/>
                  <XMRef idref="p1.m1.4"/>
                </XMApp>
                <XMWrap>
