<?latexml class="article"?>
<?latexml package="latexml"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>Evaluate At</title>
    <para xml:id="S1.p1">
      <equation xml:id="S1.Ex1">
        <Math mode="display" tex="a|_{\infty}" text="evaluated-at@(a, infinity)" xml:id="S1.Ex1.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="evaluated-at"/>
                <XMRef idref="S1.Ex1.m1.1"/>
                <XMRef idref="S1.Ex1.m1.2"/>
              </XMApp>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                <XMWrap>
                  <XMTok font="italic" role="ID" xml:id="S1.Ex1.m1.1">a</XMTok>
                  <XMTok role="CLOSE" stretchy="false">|</XMTok>
                </XMWrap>
                <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID" xml:id="S1.Ex1.m1.2">∞</XMTok>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S1.Ex2">
        <Math mode="display" tex="ab|_{a=0}cd|_{c=0}e" text="evaluated-at@(evaluated-at@(a * b, a = 0) * c * d, c = 0) * e" xml:id="S1.Ex2.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="times" role="MULOP">⁢</XMTok>
              <XMDual>
                <XMApp>
                  <XMTok meaning="evaluated-at"/>
                  <XMRef idref="S1.Ex2.m1.3"/>
                  <XMRef idref="S1.Ex2.m1.2"/>
                </XMApp>
                <XMApp>
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                  <XMWrap>
                    <XMApp xml:id="S1.Ex2.m1.3">
                      <XMTok meaning="times" role="MULOP">⁢</XMTok>
                      <XMDual>
                        <XMApp>
                          <XMTok meaning="evaluated-at"/>
                          <XMRef idref="S1.Ex2.m1.3.1"/>
                          <XMRef idref="S1.Ex2.m1.1"/>
                        </XMApp>
                        <XMApp>
                          <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                          <XMWrap>
                            <XMApp xml:id="S1.Ex2.m1.3.1">
                              <XMTok meaning="times" role="MULOP">⁢</XMTok>
                              <XMTok font="italic" role="ID">a</XMTok>
                              <XMTok font="italic" role="ID">b</XMTok>
                            </XMApp>
                            <XMTok role="CLOSE" stretchy="false">|</XMTok>
                          </XMWrap>
                          <XMApp xml:id="S1.Ex2.m1.1">
                            <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
                            <XMTok font="italic" fontsize="70%" role="ID">a</XMTok>
                            <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                          </XMApp>
                        </XMApp>
                      </XMDual>
                      <XMTok font="italic" role="ID">c</XMTok>
                      <XMTok font="italic" role="ID">d</XMTok>
                    </XMApp>
                    <XMTok role="CLOSE" stretchy="false">|</XMTok>
                  </XMWrap>
                  <XMApp xml:id="S1.Ex2.m1.2">
                    <XMTok fontsize="70%" meaning="equals" role="RELOP">=</XMTok>
                    <XMTok font="italic" fontsize="70%" role="ID">c</XMTok>
                    <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                  </XMApp>
                </XMApp>
              </XMDual>
              <XMTok font="italic" role="ID">e</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <subsection inlist="toc" xml:id="S1.SS1">
      <tags>
        <tag>1.1</tag>
        <tag role="refnum">1.1</tag>
        <tag role="typerefnum">§1.1</tag>
      </tags>
      <title><tag close=" ">1.1</tag>Conditional (probability)</title>
      <para xml:id="S1.SS1.p1">
        <equation xml:id="S1.Ex3">
          <Math mode="display" tex="a|b" text="conditional@(a, b)" xml:id="S1.Ex3.m1">
            <XMath>
              <XMApp>
                <XMTok meaning="conditional" role="MODIFIEROP" stretchy="false">|</XMTok>
                <XMTok font="italic" role="ID">a</XMTok>
                <XMTok font="italic" role="ID">b</XMTok>
              </XMApp>
            </XMath>
          </Math>
        </equation>
        <equation xml:id="S1.Ex4">
          <Math mode="display" tex="b_{a|b}" text="b _ (conditional@(a, b))" xml:id="S1.Ex4.m1">
            <XMath>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                <XMTok font="italic" role="ID">b</XMTok>
                <XMApp>
                  <XMTok fontsize="70%" meaning="conditional" role="MODIFIEROP" stretchy="false">|</XMTok>
                  <XMTok font="italic" fontsize="70%" role="ID">a</XMTok>
                  <XMTok font="italic" fontsize="70%" role="ID">b</XMTok>
                </XMApp>
              </XMApp>
            </XMath>
          </Math>
        </equation>
      </para>
    </subsection>
  </section>
  <section inlist="toc" xml:id="S2">
    <tags>
      <tag>2</tag>
      <tag role="refnum">2</tag>
      <tag role="typerefnum">§2</tag>
    </tags>
    <title><tag close=" ">2</tag>Absolute values</title>
    <para xml:id="S2.p1">
      <equation xml:id="S2.Ex5">
        <Math mode="display" tex="|a|" text="absolute-value@(a)" xml:id="S2.Ex5.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="S2.Ex5.m1.1"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMTok font="italic" role="ID" xml:id="S2.Ex5.m1.1">a</XMTok>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex6">
        <Math mode="display" tex="||a||" text="norm@(a)" xml:id="S2.Ex6.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="norm"/>
                <XMRef idref="S2.Ex6.m1.1"/>
              </XMApp>
              <XMWrap>
                <XMTok role="OPEN">||</XMTok>
                <XMTok font="italic" role="ID" xml:id="S2.Ex6.m1.1">a</XMTok>
                <XMTok role="CLOSE">||</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex7">
        <Math mode="display" tex="|a|_{\infty}" text="(absolute-value@(a)) _ infinity" xml:id="S2.Ex7.m1">
          <XMath>
            <XMApp>
              <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
              <XMDual>
                <XMApp>
                  <XMTok meaning="absolute-value"/>
                  <XMRef idref="S2.Ex7.m1.1"/>
                </XMApp>
                <XMWrap>
                  <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                  <XMTok font="italic" role="ID" xml:id="S2.Ex7.m1.1">a</XMTok>
                  <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                </XMWrap>
              </XMDual>
              <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex8">
        <Math mode="display" tex="|a+b|" text="absolute-value@(a + b)" xml:id="S2.Ex8.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="S2.Ex8.m1.1"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="S2.Ex8.m1.1">
                  <XMTok meaning="plus" role="ADDOP">+</XMTok>
                  <XMTok font="italic" role="ID">a</XMTok>
                  <XMTok font="italic" role="ID">b</XMTok>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex9">
        <Math mode="display" tex="|a-|b||" text="absolute-value@(a - absolute-value@(b))" xml:id="S2.Ex9.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="S2.Ex9.m1.2"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="S2.Ex9.m1.2">
                  <XMTok meaning="minus" role="ADDOP">-</XMTok>
                  <XMTok font="italic" role="ID">a</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="S2.Ex9.m1.1"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok font="italic" role="ID" xml:id="S2.Ex9.m1.1">b</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S2.p2">
      <equation xml:id="S2.Ex10">
        <Math mode="display" tex="\int|x|dx" text="integral@(absolute-value@(x) * differential-d@(x))" xml:id="S2.Ex10.m1">
          <XMath>
            <XMApp>
              <XMTok mathstyle="display" meaning="integral" name="int" role="INTOP">∫</XMTok>
              <XMApp>
                <XMTok meaning="times" role="MULOP">⁢</XMTok>
                <XMDual>
                  <XMApp>
                    <XMTok meaning="absolute-value"/>
                    <XMRef idref="S2.Ex10.m1.1"/>
                  </XMApp>
                  <XMWrap>
                    <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    <XMTok font="italic" role="UNKNOWN" xml:id="S2.Ex10.m1.1">x</XMTok>
                    <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                  </XMWrap>
                </XMDual>
                <XMApp>
                  <XMTok font="italic" meaning="differential-d" role="DIFFOP">d</XMTok>
                  <XMTok font="italic" role="UNKNOWN">x</XMTok>
                </XMApp>
              </XMApp>
            </XMApp>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S2.Ex11">
        <Math mode="display" tex="\exp(-p(\ln|t|+i\sin t))" text="exponential@(- p@(natural-logarithm@(absolute-value@(t)) + i * sine@(t)))" xml:id="S2.Ex11.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMRef idref="S2.Ex11.m1.3"/>
                <XMRef idref="S2.Ex11.m1.4"/>
              </XMApp>
              <XMApp>
                <XMTok meaning="exponential" role="OPFUNCTION" xml:id="S2.Ex11.m1.3">exp</XMTok>
                <XMWrap>
                  <XMTok role="OPEN" stretchy="false">(</XMTok>
                  <XMApp xml:id="S2.Ex11.m1.4">
                    <XMTok meaning="minus" role="ADDOP">-</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMRef idref="S2.Ex11.m1.2"/>
                        <XMRef idref="S2.Ex11.m1.4.1"/>
                      </XMApp>
                      <XMApp>
                        <XMTok font="italic" role="FUNCTION" xml:id="S2.Ex11.m1.2">p</XMTok>
                        <XMWrap>
                          <XMTok role="OPEN" stretchy="false">(</XMTok>
                          <XMApp xml:id="S2.Ex11.m1.4.1">
                            <XMTok meaning="plus" role="ADDOP">+</XMTok>
                            <XMApp>
                              <XMTok meaning="natural-logarithm" role="OPFUNCTION">ln</XMTok>
                              <XMDual>
                                <XMApp>
                                  <XMTok meaning="absolute-value"/>
                                  <XMRef idref="S2.Ex11.m1.1"/>
                                </XMApp>
                                <XMWrap>
                                  <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                                  <XMTok font="italic" role="UNKNOWN" xml:id="S2.Ex11.m1.1">t</XMTok>
                                  <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                                </XMWrap>
                              </XMDual>
                            </XMApp>
                            <XMApp>
                              <XMTok meaning="times" role="MULOP">⁢</XMTok>
                              <XMTok font="italic" role="UNKNOWN">i</XMTok>
                              <XMApp>
                                <XMTok meaning="sine" role="TRIGFUNCTION">sin</XMTok>
                                <XMTok font="italic" role="UNKNOWN">t</XMTok>
                              </XMApp>
                            </XMApp>
                          </XMApp>
                          <XMTok role="CLOSE" stretchy="false">)</XMTok>
                        </XMWrap>
                      </XMApp>
                    </XMDual>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">)</XMTok>
                </XMWrap>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
  <section inlist="toc" xml:id="S3">
    <tags>
      <tag>3</tag>
      <tag role="refnum">3</tag>
      <tag role="typerefnum">§3</tag>
    </tags>
    <title><tag close=" ">3</tag>Failures w/o Backtracking</title>
    <para xml:id="S3.p1">
      <equation xml:id="S3.Ex12">
        <Math mode="display" tex="||a|-|b||" text="absolute-value@(absolute-value@(a) - absolute-value@(b))" xml:id="S3.Ex12.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="S3.Ex12.m1.3"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="S3.Ex12.m1.3">
                  <XMTok meaning="minus" role="ADDOP">-</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="S3.Ex12.m1.1"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok font="italic" role="ID" xml:id="S3.Ex12.m1.1">a</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="S3.Ex12.m1.2"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok font="italic" role="ID" xml:id="S3.Ex12.m1.2">b</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex13">
        <Math mode="display" tex="||a|-b|" text="absolute-value@(absolute-value@(a) - b)" xml:id="S3.Ex13.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="absolute-value"/>
                <XMRef idref="S3.Ex13.m1.2"/>
              </XMApp>
              <XMWrap>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                <XMApp xml:id="S3.Ex13.m1.2">
                  <XMTok meaning="minus" role="ADDOP">-</XMTok>
                  <XMDual>
                    <XMApp>
                      <XMTok meaning="absolute-value"/>
                      <XMRef idref="S3.Ex13.m1.1"/>
                    </XMApp>
                    <XMWrap>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      <XMTok font="italic" role="ID" xml:id="S3.Ex13.m1.1">a</XMTok>
                      <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                    </XMWrap>
                  </XMDual>
                  <XMTok font="italic" role="ID">b</XMTok>
                </XMApp>
                <XMTok role="VERTBAR" stretchy="false">|</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math>
      </equation>
      <equation xml:id="S3.Ex14">
        <Math mode="display" tex="|a|b|_{\infty}" text="evaluated-at@(absolute-value@(a) * b, infinity)" xml:id="S3.Ex14.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="evaluated-at"/>
                <XMRef idref="S3.Ex14.m1.3"/>
                <XMRef idref="S3.Ex14.m1.2"/>
              </XMApp>
              <XMApp>
                <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                <XMWrap>
                  <XMApp xml:id="S3.Ex14.m1.3">
                    <XMTok meaning="times" role="MULOP">⁢</XMTok>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="absolute-value"/>
                        <XMRef idref="S3.Ex14.m1.1"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                        <XMTok font="italic" role="ID" xml:id="S3.Ex14.m1.1">a</XMTok>
                        <XMTok role="VERTBAR" stretchy="false">|</XMTok>
                      </XMWrap>
                    </XMDual>
                    <XMTok font="italic" role="ID">b</XMTok>
                  </XMApp>
                  <XMTok role="CLOSE" stretchy="false">|</XMTok>
                </XMWrap>
                <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID" xml:id="S3.Ex14.m1.2">∞</XMTok>
              </XMApp>
            </XMDual>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
