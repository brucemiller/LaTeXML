        </picture>
        <picture fill="black" height="100.0pt" stroke="black" tex="\begin{picture}(1.0,1.0)\put(0.0,0.0){\line(1,0){1.0}}&#10;\put(0.0,0.0){\line(1,1){0.5}}&#10;\put(0.0,0.0){\line(0,1){1.0}}&#10;\end{picture}" unitlength="100.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.Px1.p2.pic2">
