    <paragraph inlist="toc" xml:id="S2.SS0.SSS0.Px2">
      <title>Vectors:</title>
      <para xml:id="S2.SS0.SSS0.Px2.p1">
        <picture fill="black" height="100.0pt" stroke="black" tex="\begin{picture}(100.0,100.0)(-100.0,-100.0)\put(0.0,0.0){\vector(-1,0){100.0}}&#10;\put(0.0,0.0){\vector(-1,-1){100.0}}&#10;\put(0.0,0.0){\vector(0,-1){100.0}}&#10;\end{picture}" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.Px2.p1.pic1">
          <g transform="translate(100,100)">
            <g transform="translate(0,0)">
              <line points="0,0 -100,0" stroke-width="0.4" terminators="-&gt;"/>
            </g>
            <g transform="translate(0,0)">
              <line points="0,0 -100,-100" stroke-width="0.4" terminators="-&gt;"/>
            </g>
            <g transform="translate(0,0)">
              <line points="0,0 0,-100" stroke-width="0.4" terminators="-&gt;"/>
            </g>
          </g>
        </picture>
      </para>
    </paragraph>
    <paragraph inlist="toc" xml:id="S2.SS0.SSS0.Px3">
      <title>Boxes;</title>
      <para xml:id="S2.SS0.SSS0.Px3.p1">
        <picture fill="black" height="100.0pt" stroke="black" tex="\begin{picture}(100.0,100.0)\put(0.0,0.0){\bf A}&#10;\put(15.0,15.0){\framebox(15.0,15.0){A}}&#10;\put(30.0,30.0){\framebox(15.0,15.0){\bf A}}&#10;\put(45.0,45.0){\framebox(15.0,15.0){A}}&#10;\put(60.0,20.0){\frame{\shortstack{A\\&#10;B\\&#10;\bf C\\&#10;D}}}&#10;\put(75.0,20.0){\framebox(15.0,80.0){\shortstack{A\\&#10;\bf B\\&#10;C\\&#10;D}}}&#10;\end{picture}" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.Px3.p1.pic1">
          <g transform="translate(0,0)">
            <text font="bold">A</text>
          </g>
          <g transform="translate(15,15)">
            <rect fill="none" height="15" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="15" width="15">
              <text>A</text>
            </g>
          </g>
          <g transform="translate(30,30)">
            <rect fill="none" height="15" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="15" width="15">
              <text font="bold">A</text>
            </g>
          </g>
          <g transform="translate(45,45)">
            <rect fill="none" height="15" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="15" width="15">
              <text>A</text>
            </g>
          </g>
          <g transform="translate(60,20)">
            <g framed="true" stroke-width="0.4">
              <inline-block>
                <p>A</p>
                <p>B</p>
                <p><text font="bold">C</text></p>
                <p>D</p>
              </inline-block>
            </g>
          </g>
          <g transform="translate(75,20)">
            <rect fill="none" height="80" stroke-width="0.4" width="15" x="0" y="0"/>
            <g height="80" width="15">
              <inline-block>
                <p>A</p>
                <p><text font="bold">B</text></p>
                <p>C</p>
                <p>D</p>
              </inline-block>
            </g>
          </g>
        </picture>
      </para>
    </paragraph>
    <paragraph inlist="toc" xml:id="S2.SS0.SSS0.Px4">
