      <para xml:id="S2.SS0.SSS0.Px4.p1">
        <picture fill="black" height="100.0pt" stroke="black" tex="\begin{picture}(100.0,100.0)\put(30.0,30.0){\circle{30.0}\vector(0,1){15.0}%&#10;\circle*{5.0}}&#10;\put(70.0,70.0){\oval(60.0,30.0)}&#10;\end{picture}" unitlength="1.0pt" width="100.0pt" xml:id="S2.SS0.SSS0.Px4.p1.pic1">
          <g transform="translate(30,30)">
            <circle fill="none" r="15.2" stroke-width="0.4" x="0" y="0"/>
            <line points="0,0 0,15" stroke-width="0.4" terminators="-&gt;"/>
            <circle r="2.5" stroke="none" x="0" y="0"/>
          </g>
          <g transform="translate(70,70)">
            <rect fill="none" height="30" rx="15" stroke-width="0.4" width="60" x="-30" y="-15"/>
