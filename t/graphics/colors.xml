    <title><tag close=" ">6</tag>Math</title>
    <para xml:id="S6.p1">
      <equation xml:id="S6.Ex1">
        <Math mode="display" tex="{\color[rgb]{1,0,0}R}+{\color[rgb]{0,1,0}G}+{\color[rgb]{0,0,1}B}" text="R + G + B" xml:id="S6.Ex1.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok color="#FF0000" font="italic" role="UNKNOWN">R</XMTok>
              <XMTok color="#00FF00" font="italic" role="UNKNOWN">G</XMTok>
              <XMTok color="#0000FF" font="italic" role="UNKNOWN">B</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S6.p2">
      <equation xml:id="S6.Ex2">
        <Math mode="display" tex="{\pagecolor{red}R}+{\pagecolor{green}G}+{\pagecolor{blue}B}" text="R + G + B" xml:id="S6.Ex2.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok backgroundcolor="#FF0000" font="italic" role="UNKNOWN">R</XMTok>
              <XMTok backgroundcolor="#00FF00" font="italic" role="UNKNOWN">G</XMTok>
              <XMTok backgroundcolor="#0000FF" font="italic" role="UNKNOWN">B</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S6.p3">
      <equation xml:id="S6.Ex3">
        <Math mode="display" tex="\fcolorbox{green}{blue}{\textcolor{red}{R}}+\fcolorbox{blue}{red}{\textcolor{%&#10;green}{G}}+\fcolorbox{red}{green}{\textcolor{blue}{B}}" text="[R] + [G] + [B]" xml:id="S6.Ex3.m1">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMText backgroundcolor="#0000FF" framecolor="#00FF00" framed="rectangle"><text color="#FF0000">R</text></XMText>
              <XMText backgroundcolor="#FF0000" framecolor="#0000FF" framed="rectangle"><text color="#00FF00">G</text></XMText>
              <XMText backgroundcolor="#00FF00" framecolor="#FF0000" framed="rectangle"><text color="#0000FF">B</text></XMText>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
