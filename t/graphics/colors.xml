  <section refnum="6" xml:id="S6">
    <title><tag close=" ">6</tag>Math</title>
    <para xml:id="S6.p1">
      <equation xml:id="S6.Ex1">
        <Math mode="display" xml:id="S6.Ex1.m1" tex="{\color{red}R}+{\color{green}G}+{\color{blue}B}" text="R + G + B">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" font="italic" color="#FF0000">R</XMTok>
              <XMTok role="UNKNOWN" font="italic" color="#00FF00">G</XMTok>
              <XMTok role="UNKNOWN" color="#0000FF" font="italic">B</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S6.p2">
      <equation xml:id="S6.Ex2">
        <Math mode="display" xml:id="S6.Ex2.m1" tex="{\pagecolor{red}R}+{\pagecolor{green}G}+{\pagecolor{blue}B}" text="R + G + B">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMTok role="UNKNOWN" font="italic" backgroundcolor="#FF0000">R</XMTok>
              <XMTok role="UNKNOWN" backgroundcolor="#00FF00" font="italic">G</XMTok>
              <XMTok role="UNKNOWN" font="italic" backgroundcolor="#0000FF">B</XMTok>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
    <para xml:id="S6.p3">
      <equation xml:id="S6.Ex3">
        <Math mode="display" xml:id="S6.Ex3.m1" tex="\fcolorbox{green}{blue}{\textcolor{red}{R}}+\fcolorbox{blue}{red}{\textcolor{%&#10;green}{G}}+\fcolorbox{red}{green}{\textcolor{blue}{B}}" text="[R] + [G] + [B]">
          <XMath>
            <XMApp>
              <XMTok meaning="plus" role="ADDOP">+</XMTok>
              <XMText>
                <text framecolor="#00FF00" framed="rectangle" color="#FF0000" backgroundcolor="#0000FF">R</text>
              </XMText>
              <XMText>
                <text framecolor="#0000FF" framed="rectangle" color="#00FF00" backgroundcolor="#FF0000">G</text>
              </XMText>
              <XMText>
                <text framecolor="#FF0000" framed="rectangle" color="#0000FF" backgroundcolor="#00FF00">B</text>
              </XMText>
            </XMApp>
          </XMath>
        </Math>
      </equation>
    </para>
  </section>
