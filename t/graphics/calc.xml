      <p>Count 3*1.6 = 4.</p>
    </para>
    <para xml:id="S1.p3">
      <p>Count 3*1.7 = 5.</p>
    </para>
    <para xml:id="S1.p4">
      <p>Count 3*1.6*1.7 =6.</p>
    </para>
    <para xml:id="S1.p5">
      <p>Count 3+(4*5) = 23.</p>
    </para>
    <para xml:id="S1.p6">
      <p>Count 3*(4+5) = 27.</p>
    </para>
