<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <para xml:id="p1">
    <p><Math mode="inline" tex="{x}^{2}+{y}^{3}" text="x power 2 + y power 3" xml:id="p1.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
            <XMApp>
              <XMTok meaning="power" role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">x</XMTok>
              <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
            </XMApp>
            <XMApp>
              <XMTok meaning="power" role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">y</XMTok>
              <XMTok fontsize="70%" meaning="3" role="NUMBER">3</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math></p>
  </para>
  <para xml:id="p2">
    <p><Math mode="inline" tex="{f}^{\prime\prime}+{g}^{\prime\prime\prime}" text="f derivative-implicit-variable 2 + g derivative-implicit-variable 3" xml:id="p2.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="plus" role="ADDOP">+</XMTok>
            <XMApp>
              <XMTok meaning="derivative-implicit-variable" role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">f</XMTok>
              <XMDual>
                <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
                <XMWrap>
                  <XMTok fontsize="70%" name="prime" role="SUPOP">′</XMTok>
                  <XMTok fontsize="70%" name="prime" role="SUPOP">′</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
            <XMApp>
              <XMTok meaning="derivative-implicit-variable" role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">g</XMTok>
              <XMDual>
                <XMTok fontsize="70%" meaning="3" role="NUMBER">3</XMTok>
                <XMWrap>
                  <XMTok fontsize="70%" name="prime" role="SUPOP">′</XMTok>
                  <XMTok fontsize="70%" name="prime" role="SUPOP">′</XMTok>
                  <XMTok fontsize="70%" name="prime" role="SUPOP">′</XMTok>
                </XMWrap>
              </XMDual>
            </XMApp>
          </XMApp>
        </XMath>
      </Math></p>
  </para>
