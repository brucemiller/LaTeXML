<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <para xml:id="p1">
    <equation xml:id="S0.Ex1">
      <Math mode="display" tex="\underrightarrow{\mathrm{word}}\rightarrow\underleftarrow{\mathrm{word}}" text="underrightarrow@(word) rightarrow underleftarrow@(word)" xml:id="S0.Ex1.m1">
        <XMath>
          <XMApp>
            <XMTok name="rightarrow" role="ARROW">→</XMTok>
            <XMApp>
              <XMTok name="underrightarrow" role="UNDERACCENT" stretchy="true">→</XMTok>
              <XMTok role="UNKNOWN">word</XMTok>
            </XMApp>
            <XMApp>
              <XMTok name="underleftarrow" role="UNDERACCENT" stretchy="true">←</XMTok>
              <XMTok role="UNKNOWN">word</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p2">
    <equation xml:id="S0.Ex2">
      <Math mode="display" tex="\overleftrightarrow{\mathrm{word}}\leftarrow\underleftrightarrow{\mathrm{word}}" text="overleftrightarrow@(word) leftarrow underleftrightarrow@(word)" xml:id="S0.Ex2.m1">
        <XMath>
          <XMApp>
            <XMTok name="leftarrow" role="ARROW">←</XMTok>
            <XMApp>
              <XMTok name="overleftrightarrow" role="OVERACCENT" stretchy="true">↔</XMTok>
              <XMTok role="UNKNOWN">word</XMTok>
            </XMApp>
            <XMApp>
              <XMTok name="underleftrightarrow" role="UNDERACCENT" stretchy="true">↔</XMTok>
              <XMTok role="UNKNOWN">word</XMTok>
            </XMApp>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p3">
    <equation xml:id="S0.Ex3">
      <Math mode="display" tex="a^{2}\xrightarrow{\mathrm{over}}b\xleftarrow{\mathrm{over}}c" text="a ^ 2 over@(rightarrow) b over@(leftarrow) c" xml:id="S0.Ex3.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="multirelation"/>
            <XMApp>
              <XMTok role="SUPERSCRIPTOP" scriptpos="post1"/>
              <XMTok font="italic" role="UNKNOWN">a</XMTok>
              <XMTok fontsize="70%" meaning="2" role="NUMBER">2</XMTok>
            </XMApp>
            <XMApp role="ARROW">
              <XMTok fontsize="70%" role="OVERACCENT">over</XMTok>
              <XMTok name="rightarrow" role="ARROW" stretchy="true">→</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">b</XMTok>
            <XMApp role="ARROW">
              <XMTok fontsize="70%" role="OVERACCENT">over</XMTok>
              <XMTok name="leftarrow" role="ARROW" stretchy="true">←</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">c</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p4">
    <equation xml:id="S0.Ex4">
      <Math mode="display" tex="a\xrightarrow[\mathrm{under}]{\mathrm{over}}b\xleftarrow[\mathrm{under}]{%&#10;\mathrm{over}}c" text="a under@(over@(rightarrow)) b under@(over@(leftarrow)) c" xml:id="S0.Ex4.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="multirelation"/>
            <XMTok font="italic" role="UNKNOWN">a</XMTok>
            <XMApp role="ARROW">
              <XMTok fontsize="70%" role="UNDERACCENT">under</XMTok>
              <XMApp role="ARROW">
                <XMTok fontsize="70%" role="OVERACCENT">over</XMTok>
                <XMTok name="rightarrow" role="ARROW" stretchy="true">→</XMTok>
              </XMApp>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">b</XMTok>
            <XMApp role="ARROW">
              <XMTok fontsize="70%" role="UNDERACCENT">under</XMTok>
              <XMApp role="ARROW">
                <XMTok fontsize="70%" role="OVERACCENT">over</XMTok>
                <XMTok name="leftarrow" role="ARROW" stretchy="true">←</XMTok>
              </XMApp>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">c</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
  <para xml:id="p5">
    <equation xml:id="S0.Ex5">
      <Math mode="display" tex="a\overset{\mathrm{over}}{\rightarrow}b\underset{\mathrm{under}}{\leftarrow}c" text="a * over@(rightarrow) * b * under@(leftarrow) * c" xml:id="S0.Ex5.m1">
        <XMath>
          <XMApp>
            <XMTok meaning="times" role="MULOP">⁢</XMTok>
            <XMTok font="italic" role="UNKNOWN">a</XMTok>
            <XMApp>
              <XMTok fontsize="70%" role="OVERACCENT">over</XMTok>
              <XMTok name="rightarrow" role="ARROW">→</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">b</XMTok>
            <XMApp>
              <XMTok fontsize="70%" role="UNDERACCENT">under</XMTok>
              <XMTok name="leftarrow" role="ARROW">←</XMTok>
            </XMApp>
            <XMTok font="italic" role="UNKNOWN">c</XMTok>
          </XMApp>
        </XMath>
      </Math>
    </equation>
  </para>
</document>
