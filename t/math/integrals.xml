  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>Operator and range syntax</title>
    <para xml:id="S1.p1">
      <p>bare symbol
<Math mode="inline" tex="\int" text="integral" xml:id="S1.p1.m1">
          <XMath>
            <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p2">
      <p>indefinite integral
<Math mode="inline" tex="\int x\,dx" text="int@(lambda@(x, x))" xml:id="S1.p2.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="int" omcd="calculus1"/>
                <XMApp>
                  <XMTok meaning="lambda" omcd="fns1"/>
                  <XMRef idref="S1.p2.m1.2"/>
                  <XMRef idref="S1.p2.m1.1"/>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                <XMApp xml:id="S1.p2.m1.3">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.p2.m1.3.1">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt" xml:id="S1.p2.m1.1">x</XMTok>
                  <XMApp xml:id="S1.p2.m1.3.2">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S1.p2.m1.3.2.1">d</XMTok>
                    <XMTok font="italic" role="UNKNOWN" xml:id="S1.p2.m1.2">x</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p3">
      <p>definite integral
<Math mode="inline" tex="\int_{a}^{b}x\,dx" text="defint@(oriented-interval@(a, b), lambda@(x, x))" xml:id="S1.p3.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="defint" omcd="calculus1"/>
                <XMApp>
                  <XMTok meaning="oriented-interval" omcd="interval1"/>
                  <XMRef idref="S1.p3.m1.3"/>
                  <XMRef idref="S1.p3.m1.4"/>
                </XMApp>
                <XMApp>
                  <XMTok meaning="lambda" omcd="fns1"/>
                  <XMRef idref="S1.p3.m1.2"/>
                  <XMRef idref="S1.p3.m1.1"/>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                    <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                    <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S1.p3.m1.3">a</XMTok>
                  </XMApp>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S1.p3.m1.4">b</XMTok>
                </XMApp>
                <XMApp xml:id="S1.p3.m1.5">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.p3.m1.5.1">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt" xml:id="S1.p3.m1.1">x</XMTok>
                  <XMApp xml:id="S1.p3.m1.5.2">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S1.p3.m1.5.2.1">d</XMTok>
                    <XMTok font="italic" role="UNKNOWN" xml:id="S1.p3.m1.2">x</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p4">
      <p>contour integral, basic notation
<Math mode="inline" tex="\int_{C}x\,dx" text="defint@(C, lambda@(x, x))" xml:id="S1.p4.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="defint" omcd="calculus1"/>
                <XMRef idref="S1.p4.m1.3"/>
                <XMApp>
                  <XMTok meaning="lambda" omcd="fns1"/>
                  <XMRef idref="S1.p4.m1.2"/>
                  <XMRef idref="S1.p4.m1.1"/>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMApp>
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                  <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                  <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S1.p4.m1.3">C</XMTok>
                </XMApp>
                <XMApp xml:id="S1.p4.m1.4">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.p4.m1.4.1">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt" xml:id="S1.p4.m1.1">x</XMTok>
                  <XMApp xml:id="S1.p4.m1.4.2">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S1.p4.m1.4.2.1">d</XMTok>
                    <XMTok font="italic" role="UNKNOWN" xml:id="S1.p4.m1.2">x</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p5">
      <p>n-dimensional domain over reals
<Math mode="inline" tex="\int_{\mathbb{R}^{n}}x\,dx" text="defint@(R ^ n, lambda@(x, x))" xml:id="S1.p5.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="defint" omcd="calculus1"/>
                <XMRef idref="S1.p5.m1.3"/>
                <XMApp>
                  <XMTok meaning="lambda" omcd="fns1"/>
                  <XMRef idref="S1.p5.m1.2"/>
                  <XMRef idref="S1.p5.m1.1"/>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMApp>
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                  <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                  <XMApp xml:id="S1.p5.m1.3">
                    <XMTok role="SUPERSCRIPTOP" scriptpos="post3"/>
                    <XMTok font="blackboard" fontsize="70%" role="UNKNOWN">R</XMTok>
                    <XMTok font="italic" fontsize="50%" role="UNKNOWN">n</XMTok>
                  </XMApp>
                </XMApp>
                <XMApp xml:id="S1.p5.m1.4">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.p5.m1.4.1">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt" xml:id="S1.p5.m1.1">x</XMTok>
                  <XMApp xml:id="S1.p5.m1.4.2">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S1.p5.m1.4.2.1">d</XMTok>
                    <XMTok font="italic" role="UNKNOWN" xml:id="S1.p5.m1.2">x</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p6">
      <p>n-dimensional domain over range
<Math mode="inline" tex="\int_{[-\pi,\pi]^{n}}x\,dx" text="defint@((closed-interval@(- pi, pi)) ^ n, lambda@(x, x))" xml:id="S1.p6.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="defint" omcd="calculus1"/>
                <XMRef idref="S1.p6.m1.5"/>
                <XMApp>
                  <XMTok meaning="lambda" omcd="fns1"/>
                  <XMRef idref="S1.p6.m1.4"/>
                  <XMRef idref="S1.p6.m1.3"/>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMApp>
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                  <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                  <XMApp xml:id="S1.p6.m1.5">
                    <XMTok role="SUPERSCRIPTOP" scriptpos="post3"/>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="closed-interval"/>
                        <XMRef idref="S1.p6.m1.2"/>
                        <XMRef idref="S1.p6.m1.1"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok fontsize="70%" role="OPEN" stretchy="false">[</XMTok>
                        <XMApp xml:id="S1.p6.m1.2">
                          <XMTok fontsize="70%" meaning="minus" role="ADDOP">-</XMTok>
                          <XMTok font="italic" fontsize="70%" name="pi" role="UNKNOWN">π</XMTok>
                        </XMApp>
                        <XMTok fontsize="70%" role="PUNCT">,</XMTok>
                        <XMTok font="italic" fontsize="70%" name="pi" role="UNKNOWN" xml:id="S1.p6.m1.1">π</XMTok>
                        <XMTok fontsize="70%" role="CLOSE" stretchy="false">]</XMTok>
                      </XMWrap>
                    </XMDual>
                    <XMTok font="italic" fontsize="50%" role="UNKNOWN">n</XMTok>
                  </XMApp>
                </XMApp>
                <XMApp xml:id="S1.p6.m1.6">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.p6.m1.6.1">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt" xml:id="S1.p6.m1.3">x</XMTok>
                  <XMApp xml:id="S1.p6.m1.6.2">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S1.p6.m1.6.2.1">d</XMTok>
                    <XMTok font="italic" role="UNKNOWN" xml:id="S1.p6.m1.4">x</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p7">
      <p>closed contour integral, dedicated symbol
<Math mode="inline" tex="\oint_{C}\frac{1}{z}\,dz." text="contour-integral@(C, lambda@(z, 1 / z))" xml:id="S1.p7.m1">
          <XMath>
            <XMDual>
              <XMRef idref="S1.p7.m1.4"/>
              <XMWrap>
                <XMDual xml:id="S1.p7.m1.4">
                  <XMApp>
                    <XMTok meaning="contour-integral" omcd="latexml"/>
                    <XMRef idref="S1.p7.m1.3"/>
                    <XMApp>
                      <XMTok meaning="lambda" omcd="fns1"/>
                      <XMRef idref="S1.p7.m1.2"/>
                      <XMApp>
                        <XMRef idref="S1.p7.m1.1.1"/>
                        <XMRef idref="S1.p7.m1.1.2"/>
                        <XMRef idref="S1.p7.m1.1.3"/>
                      </XMApp>
                    </XMApp>
                  </XMApp>
                  <XMApp>
                    <XMApp>
                      <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                      <XMTok mathstyle="text" meaning="contour-integral" name="oint" role="INTOP">∮</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S1.p7.m1.3">C</XMTok>
                    </XMApp>
                    <XMApp xml:id="S1.p7.m1.4.1">
                      <XMTok meaning="times" role="MULOP" xml:id="S1.p7.m1.4.1.1">⁢</XMTok>
                      <XMApp rpadding="1.7pt" xml:id="S1.p7.m1.1">
                        <XMTok mathstyle="text" meaning="divide" role="FRACOP" xml:id="S1.p7.m1.1.1"/>
                        <XMTok fontsize="70%" meaning="1" role="NUMBER" xml:id="S1.p7.m1.1.2">1</XMTok>
                        <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S1.p7.m1.1.3">z</XMTok>
                      </XMApp>
                      <XMApp xml:id="S1.p7.m1.4.1.2">
                        <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S1.p7.m1.4.1.2.1">d</XMTok>
                        <XMTok font="italic" role="UNKNOWN" xml:id="S1.p7.m1.2">z</XMTok>
                      </XMApp>
                    </XMApp>
                  </XMApp>
                </XMDual>
                <XMTok role="PERIOD">.</XMTok>
              </XMWrap>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S1.p8">
      <p>loop integral
<Math mode="inline" tex="\int_{-\infty}^{(0+)}x\,dx" text="defint@(oriented-interval@(- infinity, limit-from@(0, +)), lambda@(x, x))" xml:id="S1.p8.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="defint" omcd="calculus1"/>
                <XMApp>
                  <XMTok meaning="oriented-interval" omcd="interval1"/>
                  <XMRef idref="S1.p8.m1.4"/>
                  <XMRef idref="S1.p8.m1.5"/>
                </XMApp>
                <XMApp>
                  <XMTok meaning="lambda" omcd="fns1"/>
                  <XMRef idref="S1.p8.m1.3"/>
                  <XMRef idref="S1.p8.m1.2"/>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMApp>
                  <XMTok role="SUPERSCRIPTOP" scriptpos="post2"/>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                    <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                    <XMApp xml:id="S1.p8.m1.4">
                      <XMTok fontsize="70%" meaning="minus" role="ADDOP">-</XMTok>
                      <XMTok fontsize="70%" meaning="infinity" name="infty" role="ID">∞</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMDual xml:id="S1.p8.m1.5">
                    <XMRef idref="S1.p8.m1.1"/>
                    <XMWrap>
                      <XMTok fontsize="70%" role="OPEN" stretchy="false">(</XMTok>
                      <XMApp xml:id="S1.p8.m1.1">
                        <XMTok meaning="limit-from"/>
                        <XMTok fontsize="70%" meaning="0" role="NUMBER">0</XMTok>
                        <XMTok fontsize="70%" meaning="plus" role="ADDOP">+</XMTok>
                      </XMApp>
                      <XMTok fontsize="70%" role="CLOSE" stretchy="false">)</XMTok>
                    </XMWrap>
                  </XMDual>
                </XMApp>
                <XMApp xml:id="S1.p8.m1.6">
                  <XMTok meaning="times" role="MULOP" xml:id="S1.p8.m1.6.1">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt" xml:id="S1.p8.m1.2">x</XMTok>
                  <XMApp xml:id="S1.p8.m1.6.2">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S1.p8.m1.6.2.1">d</XMTok>
                    <XMTok font="italic" role="UNKNOWN" xml:id="S1.p8.m1.3">x</XMTok>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
  </section>
  <section inlist="toc" xml:id="S2">
    <tags>
      <tag>2</tag>
      <tag role="refnum">2</tag>
      <tag role="typerefnum">§2</tag>
    </tags>
    <title><tag close=" ">2</tag>Differential localization</title>
    <para xml:id="S2.p1">
      <p>elipses in differential
<Math mode="inline" tex="\int_{\mathbb{R}^{n}}x\,d\Theta_{1}\cdots d\Theta_{n}" text="defint@(R ^ n, lambda@(Theta _ 1, cdots, Theta _ n, x))" xml:id="S2.p1.m1">
          <XMath>
            <XMDual>
              <XMApp>
                <XMTok meaning="defint" omcd="calculus1"/>
                <XMRef idref="S2.p1.m1.3"/>
                <XMApp>
                  <XMTok meaning="lambda" omcd="fns1"/>
                  <XMRef idref="S2.p1.m1.4.2.2"/>
                  <XMRef idref="S2.p1.m1.2"/>
                  <XMRef idref="S2.p1.m1.4.3.2"/>
                  <XMRef idref="S2.p1.m1.1"/>
                </XMApp>
              </XMApp>
              <XMApp>
                <XMApp>
                  <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                  <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                  <XMApp xml:id="S2.p1.m1.3">
                    <XMTok role="SUPERSCRIPTOP" scriptpos="post3"/>
                    <XMTok font="blackboard" fontsize="70%" role="UNKNOWN">R</XMTok>
                    <XMTok font="italic" fontsize="50%" role="UNKNOWN">n</XMTok>
                  </XMApp>
                </XMApp>
                <XMApp xml:id="S2.p1.m1.4">
                  <XMTok meaning="times" role="MULOP" xml:id="S2.p1.m1.4.1">⁢</XMTok>
                  <XMTok font="italic" role="UNKNOWN" rpadding="1.7pt" xml:id="S2.p1.m1.1">x</XMTok>
                  <XMApp xml:id="S2.p1.m1.4.2">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S2.p1.m1.4.2.1">d</XMTok>
                    <XMApp xml:id="S2.p1.m1.4.2.2">
                      <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                      <XMTok name="Theta" role="UNKNOWN">Θ</XMTok>
                      <XMTok fontsize="70%" meaning="1" role="NUMBER">1</XMTok>
                    </XMApp>
                  </XMApp>
                  <XMTok name="cdots" role="ID" xml:id="S2.p1.m1.2">⋯</XMTok>
                  <XMApp xml:id="S2.p1.m1.4.3">
                    <XMTok font="italic" meaning="differential-d" role="DIFFOP" xml:id="S2.p1.m1.4.3.1">d</XMTok>
                    <XMApp xml:id="S2.p1.m1.4.3.2">
                      <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                      <XMTok name="Theta" role="UNKNOWN">Θ</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN">n</XMTok>
                    </XMApp>
                  </XMApp>
                </XMApp>
              </XMApp>
            </XMDual>
          </XMath>
        </Math></p>
    </para>
    <para xml:id="S2.p2">
      <p>differential in fraction
<Math mode="inline" tex="\oint_{C}\frac{dz}{z}." text="contour-integral@(C, lambda@(z, 1 / z))" xml:id="S2.p2.m1">
          <XMath>
            <XMDual>
              <XMRef idref="S2.p2.m1.3"/>
              <XMWrap>
                <XMDual xml:id="S2.p2.m1.3">
                  <XMApp>
                    <XMTok meaning="contour-integral" omcd="latexml"/>
                    <XMRef idref="S2.p2.m1.2"/>
                    <XMApp>
                      <XMTok meaning="lambda" omcd="fns1"/>
                      <XMRef idref="S2.p2.m1.1.2.2"/>
                      <XMApp>
                        <XMRef idref="S2.p2.m1.1.1"/>
                        <XMTok font="italic" meaning="1" role="NUMBER">1</XMTok>
                        <XMRef idref="S2.p2.m1.1.3"/>
                      </XMApp>
                    </XMApp>
                  </XMApp>
                  <XMApp>
                    <XMApp>
                      <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                      <XMTok mathstyle="text" meaning="contour-integral" name="oint" role="INTOP">∮</XMTok>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S2.p2.m1.2">C</XMTok>
                    </XMApp>
                    <XMApp xml:id="S2.p2.m1.1">
                      <XMTok mathstyle="text" meaning="divide" role="FRACOP" xml:id="S2.p2.m1.1.1"/>
                      <XMApp xml:id="S2.p2.m1.1.2">
                        <XMTok meaning="times" role="MULOP" xml:id="S2.p2.m1.1.2.1">⁢</XMTok>
                        <XMTok font="italic" fontsize="70%" role="UNKNOWN">d</XMTok>
                        <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S2.p2.m1.1.2.2">z</XMTok>
                      </XMApp>
                      <XMTok font="italic" fontsize="70%" role="UNKNOWN" xml:id="S2.p2.m1.1.3">z</XMTok>
