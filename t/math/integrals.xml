  <section inlist="toc" xml:id="S3">
    <tags>
      <tag>3</tag>
      <tag role="refnum">3</tag>
      <tag role="typerefnum">§3</tag>
    </tags>
    <title><tag close=" ">3</tag>Expressions</title>
    <para xml:id="S3.p1">
      <p>contour-split of <Math mode="inline" tex="C" text="C" xml:id="S3.p1.m1">
          <XMath>
            <XMTok font="italic" role="UNKNOWN">C</XMTok>
          </XMath>
        </Math>:
<Math mode="inline" tex="\int_{\mathrm{straight}}+\int_{\mathrm{arc}}=\pi e^{-t}" text="defint@(straight, lambda@(missing-argument)) + defint@(arc, lambda@(missing-argument)) = pi * e ^ (- t)" xml:id="S3.p1.m2">
          <XMath>
            <XMApp>
              <XMTok meaning="equals" role="RELOP">=</XMTok>
              <XMApp>
                <XMTok meaning="plus" role="ADDOP">+</XMTok>
                <XMDual>
                  <XMApp>
                    <XMTok meaning="defint" omcd="calculus1"/>
                    <XMRef idref="S3.p1.m2.1"/>
                    <XMApp>
                      <XMTok meaning="lambda" omcd="fns1"/>
                      <XMTok meaning="missing-argument" omcd="ambiguous"/>
                    </XMApp>
                  </XMApp>
                  <XMApp>
                    <XMTok role="SUBSCRIPTOP" scriptpos="post2"/>
                    <XMTok mathstyle="text" meaning="integral" name="int" role="INTOP">∫</XMTok>
                    <XMTok fontsize="70%" role="UNKNOWN" xml:id="S3.p1.m2.1">straight</XMTok>
                  </XMApp>
                </XMDual>
                <XMDual>
                  <XMApp>
                    <XMTok meaning="defint" omcd="calculus1"/>
                    <XMRef idref="S3.p1.m2.2"/>
