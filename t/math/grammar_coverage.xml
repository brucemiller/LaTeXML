<?xml version="1.0" encoding="UTF-8"?>
<?latexml class="article"?>
<?latexml package="amsmath"?>
<?latexml RelaxNGSchema="LaTeXML"?>
<document xmlns="http://dlmf.nist.gov/LaTeXML">
  <resource src="LaTeXML.css" type="text/css"/>
  <resource src="ltx-article.css" type="text/css"/>
  <para xml:id="p1">
    <p>All examples in this test should be successfully parsed by LaTeXML’s grammar, and are meant as regression tests.</p>
  </para>
  <section inlist="toc" xml:id="S1">
    <tags>
      <tag>1</tag>
      <tag role="refnum">1</tag>
      <tag role="typerefnum">§1</tag>
    </tags>
    <title><tag close=" ">1</tag>sanity check</title>
    <para xml:id="S1.p1">
      <enumerate xml:id="S1.I1">
        <item xml:id="S1.I1.i1">
          <tags>
            <tag>1.</tag>
            <tag role="refnum">1</tag>
            <tag role="typerefnum">item 1</tag>
          </tags>
          <para xml:id="S1.I1.i1.p1">
            <p><Math mode="inline" tex="1+1=2" text="1 + 1 = 2" xml:id="S1.I1.i1.p1.m1">
                <XMath>
                  <XMApp>
                    <XMTok meaning="equals" role="RELOP">=</XMTok>
                    <XMApp>
                      <XMTok meaning="plus" role="ADDOP">+</XMTok>
                      <XMTok meaning="1" role="NUMBER">1</XMTok>
                      <XMTok meaning="1" role="NUMBER">1</XMTok>
                    </XMApp>
                    <XMTok meaning="2" role="NUMBER">2</XMTok>
                  </XMApp>
                </XMath>
              </Math></p>
          </para>
        </item>
      </enumerate>
    </para>
  </section>
  <section inlist="toc" xml:id="S2">
    <tags>
      <tag>2</tag>
      <tag role="refnum">2</tag>
      <tag role="typerefnum">§2</tag>
    </tags>
    <title><tag close=" ">2</tag>Single Factor</title>
    <subsubsection inlist="toc" xml:id="S2.SS0.SSS1">
      <tags>
        <tag>2.0.1</tag>
        <tag role="refnum">2.0.1</tag>
        <tag role="typerefnum">§2.0.1</tag>
      </tags>
      <title><tag close=" ">2.0.1</tag>Empty Balanced</title>
      <para xml:id="S2.SS0.SSS1.p1">
        <enumerate xml:id="S2.I1">
          <item xml:id="S2.I1.i1">
            <tags>
              <tag>1.</tag>
              <tag role="refnum">1</tag>
              <tag role="typerefnum">item 1</tag>
            </tags>
            <para xml:id="S2.I1.i1.p1">
              <p><Math mode="inline" tex="()" text="list@()" xml:id="S2.I1.i1.p1.m1">
                  <XMath>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="false">(</XMTok>
                        <XMTok role="CLOSE" stretchy="false">)</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I1.i2">
            <tags>
              <tag>2.</tag>
              <tag role="refnum">2</tag>
              <tag role="typerefnum">item 2</tag>
            </tags>
            <para xml:id="S2.I1.i2.p1">
              <p><Math mode="inline" tex="[]" text="list@()" xml:id="S2.I1.i2.p1.m1">
                  <XMath>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="false">[</XMTok>
                        <XMTok role="CLOSE" stretchy="false">]</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I1.i3">
            <tags>
              <tag>3.</tag>
              <tag role="refnum">3</tag>
              <tag role="typerefnum">item 3</tag>
            </tags>
            <para xml:id="S2.I1.i3.p1">
              <p><Math mode="inline" tex="\{\}" text="list@()" xml:id="S2.I1.i3.p1.m1">
                  <XMath>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="false">{</XMTok>
                        <XMTok role="CLOSE" stretchy="false">}</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I1.i4">
            <tags>
              <tag>4.</tag>
              <tag role="refnum">4</tag>
              <tag role="typerefnum">item 4</tag>
            </tags>
            <para xml:id="S2.I1.i4.p1">
              <p><Math mode="inline" tex="\langle\rangle" text="list@()" xml:id="S2.I1.i4.p1.m1">
                  <XMath>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok name="langle" role="OPEN" stretchy="false">⟨</XMTok>
                        <XMTok name="rangle" role="CLOSE" stretchy="false">⟩</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I1.i5">
            <tags>
              <tag>5.</tag>
              <tag role="refnum">5</tag>
              <tag role="typerefnum">item 5</tag>
            </tags>
            <para xml:id="S2.I1.i5.p1">
              <p><Math mode="inline" tex="\lfloor\rfloor" text="list@()" xml:id="S2.I1.i5.p1.m1">
                  <XMath>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok name="lfloor" role="OPEN" stretchy="false">⌊</XMTok>
                        <XMTok name="rfloor" role="CLOSE" stretchy="false">⌋</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I1.i6">
            <tags>
              <tag>6.</tag>
              <tag role="refnum">6</tag>
              <tag role="typerefnum">item 6</tag>
            </tags>
            <para xml:id="S2.I1.i6.p1">
              <p><Math mode="inline" tex="\lceil\rceil" text="list@()" xml:id="S2.I1.i6.p1.m1">
                  <XMath>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="list"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok name="lceil" role="OPEN" stretchy="false">⌈</XMTok>
                        <XMTok name="rceil" role="CLOSE" stretchy="false">⌉</XMTok>
                      </XMWrap>
                    </XMDual>
                  </XMath>
                </Math></p>
            </para>
          </item>
        </enumerate>
      </para>
    </subsubsection>
    <subsubsection inlist="toc" xml:id="S2.SS0.SSS2">
      <tags>
        <tag>2.0.2</tag>
        <tag role="refnum">2.0.2</tag>
        <tag role="typerefnum">§2.0.2</tag>
      </tags>
      <title><tag close=" ">2.0.2</tag>Monomials</title>
      <para xml:id="S2.SS0.SSS2.p1">
        <enumerate xml:id="S2.I2">
          <item xml:id="S2.I2.i1">
            <tags>
              <tag>1.</tag>
              <tag role="refnum">1</tag>
              <tag role="typerefnum">item 1</tag>
            </tags>
            <para xml:id="S2.I2.i1.p1">
              <p><Math mode="inline" tex="xyz" text="x * y * z" xml:id="S2.I2.i1.p1.m1">
                  <XMath>
                    <XMApp>
                      <XMTok meaning="times" role="MULOP">⁢</XMTok>
                      <XMTok font="italic" role="UNKNOWN">x</XMTok>
                      <XMTok font="italic" role="UNKNOWN">y</XMTok>
                      <XMTok font="italic" role="UNKNOWN">z</XMTok>
                    </XMApp>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I2.i2">
            <tags>
              <tag>2.</tag>
              <tag role="refnum">2</tag>
              <tag role="typerefnum">item 2</tag>
            </tags>
            <para xml:id="S2.I2.i2.p1">
              <p><Math mode="inline" tex="\sqrt{x}\sqrt{y}" text="square-root@(x) * square-root@(y)" xml:id="S2.I2.i2.p1.m1">
                  <XMath>
                    <XMApp>
                      <XMTok meaning="times" role="MULOP">⁢</XMTok>
                      <XMApp>
                        <XMTok meaning="square-root"/>
                        <XMTok font="italic" role="UNKNOWN">x</XMTok>
                      </XMApp>
                      <XMApp>
                        <XMTok meaning="square-root"/>
                        <XMTok font="italic" role="UNKNOWN">y</XMTok>
                      </XMApp>
                    </XMApp>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I2.i3">
            <tags>
              <tag>3.</tag>
              <tag role="refnum">3</tag>
              <tag role="typerefnum">item 3</tag>
            </tags>
            <para xml:id="S2.I2.i3.p1">
              <p><Math mode="inline" tex="\times x" text="absent * x" xml:id="S2.I2.i3.p1.m1">
                  <XMath>
                    <XMApp>
                      <XMTok meaning="times" role="MULOP">×</XMTok>
                      <XMTok meaning="absent"/>
                      <XMTok font="italic" role="UNKNOWN">x</XMTok>
                    </XMApp>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I2.i4">
            <tags>
              <tag>4.</tag>
              <tag role="refnum">4</tag>
              <tag role="typerefnum">item 4</tag>
            </tags>
            <para xml:id="S2.I2.i4.p1">
              <p><Math mode="inline" tex="\mathbin{|}x" text="|@(x)" xml:id="S2.I2.i4.p1.m1">
                  <XMath>
                    <XMApp>
                      <XMTok role="BINOP" stretchy="false">|</XMTok>
                      <XMTok font="italic" role="UNKNOWN">x</XMTok>
                    </XMApp>
                  </XMath>
                </Math></p>
            </para>
          </item>
          <item xml:id="S2.I2.i5">
            <tags>
              <tag>5.</tag>
              <tag role="refnum">5</tag>
              <tag role="typerefnum">item 5</tag>
            </tags>
          </item>
        </enumerate>
      </para>
    </subsubsection>
    <subsubsection inlist="toc" xml:id="S2.SS0.SSS3">
      <tags>
        <tag>2.0.3</tag>
        <tag role="refnum">2.0.3</tag>
        <tag role="typerefnum">§2.0.3</tag>
      </tags>
      <title><tag close=" ">2.0.3</tag>Arrays</title>
      <para xml:id="S2.SS0.SSS3.p1">
        <enumerate xml:id="S2.I3">
          <item xml:id="S2.I3.i1">
            <tags>
              <tag>1.</tag>
              <tag role="refnum">1</tag>
              <tag role="typerefnum">item 1</tag>
            </tags>
            <para xml:id="S2.I3.i1.p1">
              <p><Math mode="inline" tex="\begin{cases}a&amp;b\\&#10;c&amp;d\end{cases}" text="cases@(a, b, c, d)" xml:id="S2.I3.i1.p1.m1">
                  <XMath>
                    <XMDual>
                      <XMApp>
                        <XMTok meaning="cases"/>
                        <XMRef idref="S2.I3.i1.p1.m1.1"/>
                        <XMRef idref="S2.I3.i1.p1.m1.2"/>
                        <XMRef idref="S2.I3.i1.p1.m1.3"/>
                        <XMRef idref="S2.I3.i1.p1.m1.4"/>
                      </XMApp>
                      <XMWrap>
                        <XMTok role="OPEN" stretchy="true">{</XMTok>
                        <XMArray>
                          <XMRow>
                            <XMCell align="left">
                              <XMTok font="italic" role="UNKNOWN" xml:id="S2.I3.i1.p1.m1.1">a</XMTok>
                            </XMCell>
                            <XMCell align="left">
                              <XMTok font="italic" role="UNKNOWN" xml:id="S2.I3.i1.p1.m1.2">b</XMTok>
                            </XMCell>
                          </XMRow>
                          <XMRow>
                            <XMCell align="left">
                              <XMTok font="italic" role="UNKNOWN" xml:id="S2.I3.i1.p1.m1.3">c</XMTok>
                            </XMCell>
                            <XMCell align="left">
                              <XMTok font="italic" role="UNKNOWN" xml:id="S2.I3.i1.p1.m1.4">d</XMTok>
                            </XMCell>
                          </XMRow>
                        </XMArray>
                      </XMWrap>
                    </XMDual>
                  </XMath>
                </Math></p>
            </para>
          </item>
        </enumerate>
      </para>
    </subsubsection>
  </section>
  <section inlist="toc" xml:id="S3">
    <tags>
      <tag>3</tag>
      <tag role="refnum">3</tag>
      <tag role="typerefnum">§3</tag>
    </tags>
    <title><tag close=" ">3</tag>Edge/Artefact Cases</title>
    <para xml:id="S3.p1">
      <enumerate xml:id="S3.I1">
        <item xml:id="S3.I1.i1">
          <tags>
            <tag>1.</tag>
            <tag role="refnum">1</tag>
            <tag role="typerefnum">item 1</tag>
          </tags>
          <para xml:id="S3.I1.i1.p1">
            <p><Math mode="inline" tex="\," text="absent" xml:id="S3.I1.i1.p1.m1">
                <XMath>
                  <XMTok meaning="absent"/>
                </XMath>
              </Math></p>
          </para>
        </item>
      </enumerate>
    </para>
  </section>
</document>
